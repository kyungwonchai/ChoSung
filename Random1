Dictionary<string, int> aa = new Dictionary<string, int>();

// 딕셔너리에 예시 데이터 추가
aa.Add("키1", 10);
aa.Add("키2", 20);
aa.Add("키3", 30);

// 딕셔너리 내부 전체 값 확인
foreach (KeyValuePair<string, int> pair in aa)
{
    string key = pair.Key;
    int value = pair.Value;
    Console.WriteLine("Key: " + key + ", Value: " + value);
}
