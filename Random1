using System;
using System.Data;
using System.Linq;

public class Program
{
    public static void Main()
    {
        // DataTable 생성 및 데이터 채우기
        DataTable dataTable = new DataTable();
        dataTable.Columns.Add("수집구분", typeof(string));

        // 임의의 데이터를 추가하여 예시로 사용
        dataTable.Rows.Add("PC");
        dataTable.Rows.Add("VISA_모듈");
        dataTable.Rows.Add("PC");
        dataTable.Rows.Add("VISA_모듈");
        dataTable.Rows.Add("PC");

        // "수집구분"이 "PC"인 항목 개수 세기 (LINQ 사용)
        int 수1 = dataTable.AsEnumerable().Count(row => row.Field<string>("수집구분") == "PC");

        // "수집구분"이 "VISA_모듈"인 항목 개수 세기 (LINQ 사용)
        int 수2 = dataTable.AsEnumerable().Count(row => row.Field<string>("수집구분") == "VISA_모듈");

        // 지정된 형식의 출력
        string 출력 = $"[NOTIF 이상 수1:{수1}, 수2:{수2}]";
        Console.WriteLine(출력);
    }
}
