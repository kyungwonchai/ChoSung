using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        // 문자열 쌍을 저장하는 리스트 생성
        var stringPairs = new List<(string, string)>
        {
            ("hello", "world"),
            ("example", "pair")
            // ... 다른 문자열 쌍들
        };

        // 인덱스 1에 새로운 문자열 쌍 삽입
        stringPairs.Insert(1, ("new", "pair"));

        // 결과 출력
        foreach (var pair in stringPairs)
        {
            Console.WriteLine($"{pair.Item1}, {pair.Item2}");
        }
    }
}
