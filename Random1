def fa2(arr, n, index=0, current=[]):
    if index == len(arr):
        if len(current) == n * len(arr):
            print(current)
        return

    if len(current) < (index + 1) * n:
        for i in range(len(arr[index])):
            if arr[index][i] not in current:
                fa2(arr, n, index, current + [arr[index][i]])
    else:
        fa2(arr, n, index + 1, current)

# 예시 사용법:
arr = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
n = 2  # 각 리스트에서 2개씩 뽑기
fa2(arr, n)
