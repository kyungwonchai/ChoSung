  private void Button_Click_4(object sender, RoutedEventArgs e)
        {
             
            if (_jupyterWindow == null || !_jupyterWindow.IsLoaded)
            {
                _jupyterWindow = new JupyterStd1(@"c:\jupyterlevel4\jyputerDatascience2", @"jyputerDatascience2", "settings.ini");
                _jupyterWindow.Closed += (s, args) => { _jupyterWindow = null; };  // 창이 닫히면 필드를 null로 설정
                _jupyterWindow.Show();
            }
            else
            {
                // 이미 열려있는 창을 활성화
                _jupyterWindow.Activate();
            }

        }