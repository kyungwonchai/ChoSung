namespace AAutoHandle.MacroActions
{
    using System;
    using System.Threading.Tasks;
    using AAutoHandle.Core;
    using AAutoHandle.Commands;
    using OpenQA.Selenium;
    using OpenQA.Selenium.Chrome;

    public class BrowserActionManager
    {
        private IWebDriver _driver;

        public async Task OpenBrowserAsync(string url)
        {
            try
            {
                _driver = new ChromeDriver();
                _driver.Navigate().GoToUrl(url);
                MacroLogger.Instance.AddLog($"ë¸Œë¼ìš°ì € ì—´ê¸°: {url}");
                await Task.Delay(2000); // í˜ì´ì§€ ë¡œë”© ëŒ€ê¸°
            }
            catch (Exception ex)
            {
                MacroLogger.Instance.AddLog($"[Error] ë¸Œë¼ìš°ì € ì‹¤í–‰ ì‹¤íŒ¨: {ex.Message}");
            }
        }

        public async Task ClickElementAsync(string elementXPath)
        {
            try
            {
                var element = _driver.FindElement(By.XPath(elementXPath));
                element.Click();
                MacroLogger.Instance.AddLog($"ìš”ì†Œ í´ë¦­: {elementXPath}");
                await Task.Delay(500); // í´ë¦­ í›„ ëŒ€ê¸°
            }
            catch (Exception ex)
            {
                MacroLogger.Instance.AddLog($"[Error] í´ë¦­ ì‹¤íŒ¨: {ex.Message}");
            }
        }

        public async Task InputTextAsync(string elementXPath, string text)
        {
            try
            {
                var element = _driver.FindElement(By.XPath(elementXPath));
                element.SendKeys(text);
                MacroLogger.Instance.AddLog($"í…ìŠ¤íŠ¸ ì…ë ¥: {text}");
                await Task.Delay(500); // ì…ë ¥ í›„ ëŒ€ê¸°
            }
            catch (Exception ex)
            {
                MacroLogger.Instance.AddLog($"[Error] í…ìŠ¤íŠ¸ ì…ë ¥ ì‹¤íŒ¨: {ex.Message}");
            }
        }

        public void CloseBrowser()
        {
            try
            {
                _driver?.Quit();
                MacroLogger.Instance.AddLog("ë¸Œë¼ìš°ì € ì¢…ë£Œ");
            }
            catch (Exception ex)
            {
                MacroLogger.Instance.AddLog($"[Error] ë¸Œë¼ìš°ì € ì¢…ë£Œ ì‹¤íŒ¨: {ex.Message}");
            }
        }
    }
}
