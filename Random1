using System;
using System.Drawing;
using System.Windows.Forms;

namespace ColorClickExample
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void CheckColorButton_Click(object sender, EventArgs e)
        {
            Color targetColor = Color.FromArgb(38, 99, 0); // 대상 색상 (예: 38, 99, 0)

            if (targetColor.R < 80 && targetColor.G < 80 && targetColor.B < 80)
            {
                // 모든 RGB 값이 80 미만인 경우 클릭 이벤트를 처리합니다.
                MessageBox.Show("색상이 모두 80 미만입니다. 버튼이 클릭되었습니다!");
            }
        }
    }
}
