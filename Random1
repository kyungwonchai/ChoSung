#include <iostream>

// 기본 클래스 Animal 선언
class Animal {
public:
    // 가상 함수 Speak 선언
    virtual void Speak() {
        std::cout << "This is an animal." << std::endl;
    }
};

// 파생 클래스 Dog, Animal을 상속받음
class Dog : public Animal {
public:
    // Speak 함수 재정의
    void Speak() override {
        std::cout << "Woof! Woof!" << std::endl;
    }
};

// 파생 클래스 Cat, Animal을 상속받음
class Cat : public Animal {
public:
    // Speak 함수 재정의
    void Speak() override {
        std::cout << "Meow! Meow!" << std::endl;
    }
};

int main() {
    Animal* animal = new Animal();  // Animal 객체 생성
    Animal* dog = new Dog();  // Dog 객체 생성
    Animal* cat = new Cat();  // Cat 객체 생성

    animal->Speak();  // "This is an animal." 출력
    dog->Speak();  // "Woof! Woof!" 출력
    cat->Speak();  // "Meow! Meow!" 출력

    delete animal;
    delete dog;
    delete cat;

    return 0;
}
