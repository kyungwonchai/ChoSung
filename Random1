Sub AssignWorkbookToVariable()
    Dim wb As Workbook
    Dim wb2 As Workbook
    Dim found As Boolean

    found = False

    ' 모든 열려있는 워크북을 확인
    For Each wb In Application.Workbooks
        ' 파일 이름이 "SOP"로 시작하는지 확인
        If Left(wb.Name, 3) = "SOP" Then
            Set wb2 = wb
            found = True
            Exit For
        End If
    Next wb

    ' SOP로 시작하는 워크북을 찾았는지 확인
    If found Then
        MsgBox "SOP로 시작하는 파일을 찾았습니다: " & wb2.Name
    Else
        MsgBox "SOP로 시작하는 파일이 열려 있지 않습니다."
    End If
End Sub
