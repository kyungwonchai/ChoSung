private void pivotGridControl1_CustomCellStyle(object sender, PivotCustomCellStyleEventArgs e)
{
    if (e.ColumnField.FieldName.StartsWith("Mounter") && !e.Item.IsDataField)
    {
        if (e.ColumnField.GetFieldValue(e.Item) != null)
        {
            e.Appearance.BackColor = Color.Yellow;
        }
    }
}
