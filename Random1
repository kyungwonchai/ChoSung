SELECT 
    h.m8, h.m7, m.ind, 
    m.stabilizedtime_b, 
    m.stabilizedtime_b_start, 
    TIMEDIFF(m.stabilizedtime_b, m.stabilizedtime_b_start) as timediff_result
FROM 
    smd.tbl_history AS h
JOIN 
    smd.tbl_modelchange AS m 
ON 
    COALESCE(h.m8, h.m7) = m.ind
ORDER BY 
    TIMEDIFF(m.stabilizedtime_b, m.stabilizedtime_b_start) DESC;
