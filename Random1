UPDATE A0_MCH_History
SET
    modelchangetime = pgmdownloadingtime,
    stabilizedtime_b = pgmdownloadingtime,
    stabilizedtime_b_start = pgmdownloadingtime,
    b2 = pgmdownloadingtime,
    b3 = pgmdownloadingtime,
    b4 = pgmdownloadingtime,
    b5 = pgmdownloadingtime,
    b6 = pgmdownloadingtime,
    b7 = pgmdownloadingtime,
    b8 = pgmdownloadingtime,
    b9 = pgmdownloadingtime
WHERE modelchangetime < pgmdownloadingtime;
