file_path = r"d:\ctem\for_sss.data"  # 절대 경로를 사용할 때는 'r' 접두사를 붙여 이스케이프 문자를 무시합니다

try:
    with open(file_path, "r") as file:
        data = file.read()
        # 파일 데이터에 대한 처리 작업 수행
        print(data)
except FileNotFoundError:
    print("파일을 찾을 수 없습니다.")
except IOError:
    print("파일을 읽는 동안 오류가 발생했습니다.")
