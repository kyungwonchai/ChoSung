private void gridView_RowCellStyle(object sender, DevExpress.XtraGrid.Views.Grid.RowCellStyleEventArgs e)
{
    if (sender is GridView gridView)
    {
        object value = gridView.GetRowCellValue(e.RowHandle, "컬럼A");

        if (value != null && int.TryParse(value.ToString(), out int numericValue) && numericValue > 60)
        {
            e.Appearance.BackColor = Color.Yellow; // 노랑으로 변경
        }
    }
}
