using System;
using System.IO;
using System.Text.RegularExpressions;

class Program
{
    static void Main()
    {
        string path = "your_file_path_here.txt"; // 여기에 파일 경로를 입력하세요.
        int 변수A = 0;
        string targetString = "";

        using (StreamReader sr = new StreamReader(path))
        {
            string line;
            while ((line = sr.ReadLine()) != null)
            {
                if (line.Contains("[1:0:BSM"))
                {
                    변수A = 1;
                }
                else if (line.Contains("[0:0:BSM"))
                {
                    변수A = 2;
                }

                Regex regex = new Regex(@"BSM-(.+)\]");
                Match match = regex.Match(line);

                if (match.Success)
                {
                    targetString = match.Groups[1].Value;
                }
            }
        }

        Console.WriteLine("변수A: " + 변수A);
        Console.WriteLine("Target String: " + targetString);
    }
}
