using System;
using System.Diagnostics;
using System.Windows.Forms;

namespace YourNamespace
{
    public partial class MainForm : Form
    {
        public MainForm()
        {
            InitializeComponent();
        }

        private void button1_Click(object sender, EventArgs e)
        {
            ProcessStartInfo startInfo = new ProcessStartInfo();
            startInfo.FileName = "your_executable.exe";  // 실행할 프로세스 파일 경로
            startInfo.CreateNoWindow = true;  // 검은색 커맨드 창 표시 안 함
            // 추가적인 설정을 원한다면 필요한 속성들을 설정합니다.
            // startInfo.Arguments = "your_arguments";  // 실행 인수
            // startInfo.WorkingDirectory = "working_directory";  // 작업 디렉토리

            Process.Start(startInfo);
        }
    }
}
