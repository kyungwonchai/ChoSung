# -*- mode: python ; coding: utf-8 -*-

block_cipher = None

# 경로는 시스템에 따라 달라질 수 있으므로 확인이 필요합니다.
# 아래 경로는 예시일 뿐 실제 환경에 맞게 수정해야 합니다.
openpyxl_path = '/path_to_python/Lib/site-packages/openpyxl'

a = Analysis(['uppy1.py'],
             pathex=['/your/path/to/uppy1'],
             binaries=[],
             datas=[(openpyxl_path, 'openpyxl')],
             hiddenimports=['openpyxl'],
             hookspath=[],
             runtime_hooks=[],
             excludes=[],
             win_no_prefer_redirects=False,
             win_private_assemblies=False,
             cipher=block_cipher,
             noarchive=False)

pyz = PYZ(a.pure, a.zipped_data,
             cipher=block_cipher)

exe = EXE(pyz,
          a.scripts,
          [],
          exclude_binaries=True,
          name='uppy1',
          debug=False,
          bootloader_ignore_signals=False,
          strip=False,
          upx=True,
          runtime_tmpdir=None,
          console=True )

coll = COLLECT(exe,
               a.binaries,
               a.zipfiles,
               a.datas,
               strip=False,
               upx=True,
               upx_exclude=[],
               name='uppy1')
