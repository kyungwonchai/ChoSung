using DevExpress.XtraDiagram;
using System.Drawing;

// ...

private void SetDiagramBackgroundImage()
{
    string imagePath = @"C:\agent\1.jpg"; // 이미지 파일 경로

    // 이미지 파일을 로드
    Image backgroundImage = Image.FromFile(imagePath);

    // Diagram Control의 배경 이미지 설정
    diagramControl1.BackgroundImage = backgroundImage;

    // 이미지를 기준으로 Diagram Control의 크기를 조정 (선택 사항)
    diagramControl1.Size = backgroundImage.Size;
}
