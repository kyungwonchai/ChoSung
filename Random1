CREATE PROCEDURE CheckRowCount
AS
BEGIN
    -- 변수 선언
    DECLARE @previousCount INT;
    DECLARE @currentCount INT;

    -- 초기값 설정
    SET @previousCount = 0;
    SET @currentCount = (SELECT COUNT(*) FROM a1_기변시점);

    -- While loop를 통해 현재 카운트가 이전 카운트와 동일하면 종료, 그렇지 않으면 계속 반복
    WHILE @previousCount <> @currentCount
    BEGIN
        -- 이전 카운트 업데이트
        SET @previousCount = @currentCount;
        
        -- 여기에 반복할 작업을 삽입하세요.
        
        -- 현재 카운트 업데이트
        SET @currentCount = (SELECT COUNT(*) FROM a1_기변시점);
    END
END
