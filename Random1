private void CopyTextToAnotherForm()
{
    F2Form f2Form = new F2Form();
    f2Form.Show();

    // textBox1의 내용을 한 글자씩 처리합니다.
    foreach (char character in textBox1.Text)
    {
        // 대상 폼 및 컨트롤에 포커스를 설정합니다.
        f2Form.Activate();
        f2Form.textBox2.Focus();

        // SendKeys를 사용하여 문자를 전송합니다.
        SendKeys.SendWait(character.ToString());

        // 필요한 경우 딜레이를 추가할 수 있습니다.
        // System.Threading.Thread.Sleep(100);
    }
}
