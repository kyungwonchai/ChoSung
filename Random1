// Assuming you have a TableView named "tableView"
tableView.CellStyle += TableView_CellStyle;

private void TableView_CellStyle(object sender, CellStyleEventArgs e)
{
    if (e.Column.FieldName == "re5" && e.Value != null)
    {
        if (Convert.ToDouble(e.Value) == 99.73)
        {
            // For value 99.73, you might not want to change the background color.
        }
        else
        {
            // Sets the background color to LightYellow if the value is not 99.73
            e.Appearance.Background = System.Windows.Media.Brushes.LightYellow;
        }
    }
    else if (e.Column.FieldName == "re7" && e.Value != null && Convert.ToDouble(e.Value) < 6)
    {
        // Sets the background color to LightPink if the value is less than 6
        e.Appearance.Background = System.Windows.Media.Brushes.LightPink;
    }
}
