string b = string.Empty;  // 초기값 설정
string previousValue = string.Empty;  // 이전 값을 저장할 변수 설정

foreach (var item in someCollection)  // someCollection은 당신의 컬렉션 이름이어야 합니다.
{
    string newValue = GetNewValue(item);  // GetNewValue는 당신이 새 값을 얻는 메서드이어야 합니다.

    // 새 값이 비어 있지 않은 경우에만 변수 b를 업데이트하고 이전 값을 저장
    if (!string.IsNullOrEmpty(newValue))
    {
        previousValue = newValue;
        b = newValue;
    }
    else  // 새 값이 비어 있는 경우, 변수 b를 이전 값으로 설정
    {
        b = previousValue;
    }
}
