using System;
using System.Text.RegularExpressions;

namespace YourNamespace
{
    class Program
    {
        static void Main(string[] args)
        {
            string input = "예제123!@#abc";

            if (Regex.IsMatch(input, @"[^a-zA-Z0-9\p{P}\p{S}]"))
            {
                Console.WriteLine("문자열에 영어, 숫자, 특수문자 외의 문자가 포함되어 있습니다.");
            }
            else
            {
                Console.WriteLine("문자열은 영어, 숫자, 특수문자만 포함하고 있습니다.");
            }
        }
    }
}
