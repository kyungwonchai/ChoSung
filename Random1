using System;
using System.Collections.Generic;
using System.Net;

class Program
{
    static void Main()
    {
        // IP 주소 리스트 선언
        List<IPAddress> ipList = new List<IPAddress>
        {
            IPAddress.Parse("192.168.0.1"),
            IPAddress.Parse("192.168.0.2"),
            IPAddress.Parse("192.168.0.3")
        };

        // 체크할 IP 주소
        string ipToCheck = "192.168.0.2";

        // IP 주소를 IPAddress 형식으로 변환
        IPAddress ipAddressToCheck = IPAddress.Parse(ipToCheck);

        // IP 주소가 리스트에 포함되어 있는지 확인
        bool isContained = ipList.Contains(ipAddressToCheck);

        // 결과 출력
        if (isContained)
        {
            Console.WriteLine($"{ipToCheck} is contained in the IP list.");
        }
        else
        {
            Console.WriteLine($"{ipToCheck} is not contained in the IP list.");
        }
    }
}
