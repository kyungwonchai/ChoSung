-- MainTable
CREATE TABLE MainTable (
    UniqueKey VARCHAR(255) PRIMARY KEY,
    [Date] DATE,
    WorkGroup CHAR(1),
    Line CHAR(1),
    Model VARCHAR(50),
    ResponsibleDept VARCHAR(50),
    Array INT,
    BoardQuantityB INT,
    BoardQuantityT INT,
    TotalQuantity INT,
    Reason NVARCHAR(255),
    ActionTaken NVARCHAR(255),
    ActionTaker NVARCHAR(10),
    IssueType VARCHAR(50),
    Quantity INT,
    IsHighValueMaterial CHAR(1),
    -- 다른 필드들
);

-- ModelTable
CREATE TABLE ModelTable (
    ModelID INT PRIMARY KEY,
    ModelName VARCHAR(50)
    -- 다른 필드들
);

-- IssueTypeTable
CREATE TABLE IssueTypeTable (
    IssueTypeID INT PRIMARY KEY,
    IssueTypeName VARCHAR(50)
    -- 다른 필드들
);

-- ReturnMaterialStatus
CREATE TABLE ReturnMaterialStatus (
    UniqueKey VARCHAR(255),
    MaterialCode VARCHAR(50),
    ReturnQuantity INT,
    MissingQuantity INT,
    HiddenColumn VARCHAR(50),
    -- 다른 필드들
);

-- AuditTable
CREATE TABLE AuditTable (
    AuditID INT PRIMARY KEY,
    Timestamp DATETIME,
    UniqueKey VARCHAR(255),
    Action VARCHAR(10)
    -- 다른 필드들
);
