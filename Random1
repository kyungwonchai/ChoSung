// ribbonControl: 현재 폼에 있는 RibbonControl의 인스턴스

foreach (RibbonPage page in ribbonControl.Pages)
{
    foreach (RibbonPageGroup group in page.Groups)
    {
        foreach (BarItemLink link in group.ItemLinks)
        {
            // link.Item을 BarButtonItem으로 캐스팅 시도
            if (link.Item is BarButtonItem buttonItem)
            {
                // buttonItem을 사용하여 원하는 작업 수행
                Console.WriteLine($"Button Item Name: {buttonItem.Name}");
            }
            else
            {
                // link.Item이 BarButtonItem이 아닌 경우 처리
                Console.WriteLine($"Item '{link.Item.Name}' is of type {link.Item.GetType()} and is not a BarButtonItem.");
            }
        }
    }