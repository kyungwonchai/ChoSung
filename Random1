using System;
using System.Collections.Generic;
using System.Diagnostics;
using System.Linq;

class Program
{
    static void Main(string[] args)
    {
        List<Tuple<string, int, int>> processList = new List<Tuple<string, int, int>>();

        foreach (Process process in Process.GetProcesses())
        {
            try
            {
                int pid = process.Id;
                string processName = process.ProcessName;
                int handleCount = process.HandleCount;

                processList.Add(new Tuple<string, int, int>(processName, pid, handleCount));
            }
            catch (Exception ex)
            {
                // 접근 제한이 있는 프로세스는 무시
            }
        }

        // 프로세스 이름으로 오름차순 정렬
        var sortedList = processList.OrderBy(p => p.Item1);

        // 결과 출력
        foreach (var item in sortedList)
        {
            Console.WriteLine($"ProcessName: {item.Item1}, PID: {item.Item2}, HandleCount: {item.Item3}");
        }
    }
}
