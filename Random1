sudo sh -c 'cat <<EOF > /etc/apt/sources.list
deb http://mirror.kakao.com/ubuntu/ noble main restricted universe multiverse
deb http://mirror.kakao.com/ubuntu/ noble-updates main restricted universe multiverse
deb http://mirror.kakao.com/ubuntu/ noble-backports main restricted universe multiverse
deb http://security.ubuntu.com/ubuntu/ noble-security main restricted universe multiverse
EOF'