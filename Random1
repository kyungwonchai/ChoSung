Sub FillBlueForTransparentOrWhiteCellsWithRowHeight()
    Dim ws As Worksheet
    Dim lastRow As Long
    Dim cell As Range
    Dim rowHeight As Double

    ' 이미 설정된 변수 주차2_시트를 사용
    Set ws = 주차2_시트
    
    ' D4부터 마지막 데이터가 있는 행까지 (End(xlUp) 사용)
    lastRow = ws.Cells(ws.Rows.Count, "D").End(xlUp).Row
    
    ' D4부터 마지막 데이터가 있는 행까지 반복
    For Each cell In ws.Range("D4:D" & lastRow)
        ' 행 높이 가져오기
        rowHeight = cell.RowHeight
        
        ' 배경색이 투명하거나 희색인지 확인
        If cell.Interior.Color = xlNone Or cell.Interior.Color = RGB(255, 255, 255) Then
            ' 배경을 파란색으로 설정
            cell.Interior.Color = RGB(0, 0, 255)
        End If
    Next cell
    
    MsgBox "파란색으로 칠하기 작업이 완료되었습니다.", vbInformation