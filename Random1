using System.Diagnostics;

// ...

private void KillExcelProcesses()
{
    try
    {
        Process[] processes = Process.GetProcessesByName("excel");

        foreach (Process process in processes)
        {
            process.Kill();
            process.WaitForExit();
            process.Dispose();
        }
    }
    catch (Exception ex)
    {
        // Excel 프로세스 종료 중에 예외 처리
        MessageBox.Show($"Excel 프로세스 종료 중 오류가 발생했습니다: {ex.Message}");
    }
}

private void DeleteFilesStartingWith(string folderPath, string fileNamePrefix)
{
    // Excel 프로세스 종료
    KillExcelProcesses();

    try
    {
        string[] files = Directory.GetFiles(folderPath, $"{fileNamePrefix}*");

        foreach (string file in files)
        {
            File.Delete(file);
        }
    }
    catch (Exception ex)
    {
        // 파일 삭제 실패 시 예외 처리
        MessageBox.Show($"파일 삭제 중 오류가 발생했습니다: {ex.Message}");
    }
}
