using System;
using System.Text.RegularExpressions;

class Program
{
    static void Main()
    {
        // Example string to check
        string inputString = "example_01_123.crb";
        
        // Regular expression pattern to match "_01_" followed by any digits and ending with ".crb"
        string pattern = @"_01_\d+\.crb$";
        
        // Check if the string matches the pattern
        bool matches = Regex.IsMatch(inputString, pattern);
        
        if (matches)
        {
            Console.WriteLine("The string matches the pattern.");
        }
        else
        {
            Console.WriteLine("The string does not match the pattern.");
        }
    }
}
