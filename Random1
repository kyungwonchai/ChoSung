using MySql.Data.MySqlClient;

class Program
{
    static void Main()
    {
        string connectionString = "server=<서버주소>;port=<포트번호>;database=<데이터베이스명>;user=<사용자명>;password=<비밀번호>";

        // MySQL 연결 생성
        MySqlConnection connection = new MySqlConnection(connectionString);

        try
        {
            // MySQL 연결 열기
            connection.Open();

            // INSERT 쿼리 작성
            string query = "INSERT INTO 테이블명 (열1, 열2, 열3) VALUES (@열1, @열2, @열3)";

            // MySQL 쿼리 커맨드 생성
            MySqlCommand command = new MySqlCommand(query, connection);

            // 파라미터 추가
            command.Parameters.AddWithValue("@열1", "값1");
            command.Parameters.AddWithValue("@열2", "값2");
            command.Parameters.AddWithValue("@열3", "값3");

            // 쿼리 실행
            int rowsAffected = command.ExecuteNonQuery();

            // 영향 받은 행 수 확인
            Console.WriteLine("영향 받은 행 수: " + rowsAffected);
        }
        catch (Exception ex)
        {
            Console.WriteLine("에러: " + ex.Message);
        }
        finally
        {
            // MySQL 연결 닫기
            connection.Close();
        }
    }
}
