def bytes_to_hex(data):
    return ' '.join(format(byte, '02X') for byte in data)

with open('system.stm', 'rb') as file:
    content = file.read()
    hex_content = bytes_to_hex(content)
    print(hex_content)

    ascii_content = ''.join(chr(byte) if 32 <= byte <= 126 else '.' for byte in content)
    print(ascii_content)
