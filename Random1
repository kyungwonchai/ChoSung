using Excel = Microsoft.Office.Interop.Excel;

// ...

public int FindLastRowWithValuesInColumnC(string filePath)
{
    Excel.Application excelApp = new Excel.Application();
    Excel.Workbook workbook = excelApp.Workbooks.Open(filePath);
    Excel.Worksheet worksheet = workbook.ActiveSheet;

    Excel.Range columnC = worksheet.Range["C1:C" + worksheet.Cells[worksheet.Rows.Count, "C"].End[Excel.XlDirection.xlUp].Row];

    int lastRow = 0;
    foreach (Excel.Range cell in columnC)
    {
        if (cell.Value != null && cell.Value.ToString().Length >= 2)
        {
            lastRow = cell.Row;
        }
    }

    // Close the workbook and quit Excel application
    workbook.Close();
    excelApp.Quit();

    // Release COM objects
    System.Runtime.InteropServices.Marshal.ReleaseComObject(columnC);
    System.Runtime.InteropServices.Marshal.ReleaseComObject(worksheet);
    System.Runtime.InteropServices.Marshal.ReleaseComObject(workbook);
    System.Runtime.InteropServices.Marshal.ReleaseComObject(excelApp);

    return lastRow;
}
