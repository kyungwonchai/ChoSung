using Excel = Microsoft.Office.Interop.Excel;

// Excel 애플리케이션 객체 생성
var excelApp = new Excel.Application();
excelApp.Visible = false; // Excel을 보이지 않게 설정

// Excel 워크북 열기
Excel.Workbook workbook = excelApp.Workbooks.Open("C:\\path\\to\\your\\file.xlsx");

// 첫 번째 워크시트를 가져옴
Excel._Worksheet worksheet = (Excel._Worksheet)workbook.Sheets[1];

// 워크시트 이름을 "BCD"로 변경
worksheet.Name = "BCD";

// 셀 1,1을 시작점으로 하는 UsedRange를 인쇄 영역으로 설정
Excel.Range usedRange = worksheet.UsedRange;
string usedRangeAddress = usedRange.get_Address(true, true, Excel.XlReferenceStyle.xlA1, Type.Missing, Type.Missing);
worksheet.PageSetup.PrintArea = usedRangeAddress;

// 변경 사항을 저장하고 Excel을 닫음
workbook.Save();
workbook.Close();
excelApp.Quit();

// COM 객체 해제
System.Runtime.InteropServices.Marshal.ReleaseComObject(worksheet);
System.Runtime.InteropServices.Marshal.ReleaseComObject(workbook);
System.Runtime.InteropServices.Marshal.ReleaseComObject(excelApp);
