using System;
using System.Data;

namespace DataTableExample
{
    public partial class MainForm : Form
    {
        public MainForm()
        {
            InitializeComponent();
        }

        private void btnCount_Click(object sender, EventArgs e)
        {
            // DataTable 생성 및 예시 데이터 추가
            DataTable dt = new DataTable();
            dt.Columns.Add("집계시간", typeof(int));
            dt.Rows.Add(2);
            dt.Rows.Add(4);
            dt.Rows.Add(5);
            dt.Rows.Add(7);
            dt.Rows.Add(10);

            // 5 이상인 값을 가진 행의 개수를 저장할 변수
            int count = 0;

            // "집계시간" 컬럼의 값이 5 이상인 행을 세기
            foreach (DataRow row in dt.Rows)
            {
                if ((int)row["집계시간"] >= 5)
                {
                    count++;
                }
            }

            // 결과 출력
            MessageBox.Show("5 이상인 값의 개수: " + count.ToString());
        }
    }
}
