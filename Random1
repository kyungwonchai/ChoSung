private void gridControl1_CustomDrawCell(object sender, DevExpress.XtraGrid.Views.Base.RowCellCustomDrawEventArgs e)
{
    if (e.Column.VisibleIndex >= 4) // 4번째 이후의 컬럼인지 확인
    {
        string cellValue = e.CellValue.ToString();
        string[] splitValues = cellValue.Split('(');

        if (splitValues.Length > 0 && splitValues[0] != "100") // ( 로 나눈 첫 번째 문자열이 "100"이 아닌 경우
        {
            e.Appearance.BackColor = Color.Yellow; // 노랑으로 표시
        }
    }
}
