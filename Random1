using Microsoft.Extensions.Logging;

namespace MyApp
{
    public class Program
    {
        public static void Main(string[] args)
        {
            // Logger를 설정합니다.
            var logger = LoggerFactory.CreateLogger(typeof(Program));

            // 로그를 누적합니다.
            logger.Log(LogLevel.Information, "This is a test message.", LogFile("c:\\agent\\weblog\\1.txt"));

            // 프로그램을 종료합니다.
            Environment.Exit(0);
        }
    }
}