public partial class App : Application
{
    protected override void OnStartup(StartupEventArgs e)
    {
        base.OnStartup(e);

        // 필요한 객체들 생성
        TaskQueueViewModel taskQueueViewModel = new TaskQueueViewModel();
        AnotherClass anotherClass = new AnotherClass();

        // MainViewModel에 필요한 의존성을 주입
        MainViewModel mainViewModel = new MainViewModel(taskQueueViewModel, anotherClass);

        // View와 ViewModel 연결
        MainWindow window = new MainWindow();
        window.DataContext = mainViewModel;
        window.Show();
    }
}
