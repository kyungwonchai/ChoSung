private void barLineChart_CustomDrawSeriesPoint(object sender, CustomDrawSeriesPointEventArgs e)
{
    // 막대 차트의 포인트만 고려합니다.
    if (e.Series.Name == "DDSeries2")
    {
        // 선 차트의 해당 포인트를 찾습니다.
        SeriesPoint linePoint = DDSeries3.Points.FirstOrDefault(p => p.Argument == e.SeriesPoint.Argument);

        // 선 차트의 포인트가 있는지, 그리고 그 값이 90보다 작은지 확인합니다.
        if (linePoint != null && double.TryParse(linePoint.Values[0].ToString(), out double lineValue) && lineValue < 90)
        {
            // 선 차트의 값이 90보다 작으면 막대의 색상을 주황색으로 변경합니다.
            e.SeriesDrawOptions.Color = Color.Orange;
        }
        else
        {
            // 그렇지 않으면 막대의 색상을 연두색으로 변경합니다.
            e.SeriesDrawOptions.Color = Color.LightGreen;
        }
    }
}
