using System;
using System.IO;

class Program
{
    static void Main()
    {
        string fullPath = @"c:\sdgsg\t325"; // 전체 경로
        string folderName = Path.GetFileName(fullPath.TrimEnd(Path.DirectorySeparatorChar, Path.AltDirectorySeparatorChar));
        
        Console.WriteLine(folderName); // t325 출력
    }
}
