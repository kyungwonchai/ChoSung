-- 가정: ModelName 컬럼의 예시 값 'AB_CD_EF_GH'
-- 목표: 'EF' 세그먼트에서 세 번째('F')와 네 번째 글자(해당 예시에서는 없음) 추출

-- ModelName 컬럼에서 언더스코어로 분리된 세 번째 세그먼트의 세 번째 및 네 번째 글자 추출
SELECT
    ModelName,
    SUBSTRING(ThirdSegment, 3, 1) AS ThirdChar,  -- 세 번째 글자
    SUBSTRING(ThirdSegment, 4, 1) AS FourthChar  -- 네 번째 글자
FROM (
    SELECT
        ModelName,
        -- SUBSTRING과 CHARINDEX를 사용하여 세 번째 세그먼트 추출
        SUBSTRING(ModelName, CHARINDEX('_', ModelName, CHARINDEX('_', ModelName) + 1) + 1, 
        CHARINDEX('_', ModelName, CHARINDEX('_', ModelName, CHARINDEX('_', ModelName) + 1) + 1) - 
        CHARINDEX('_', ModelName, CHARINDEX('_', ModelName) + 1) - 1) AS ThirdSegment
    FROM YourTable
) AS DerivedTable
