SELECT 
  CONVERT(DATETIME, SUBSTRING(filename, 1, 4) + '-' + 
          SUBSTRING(filename, 5, 2) + '-' + 
          SUBSTRING(filename, 7, 2) + ' ' + 
          SUBSTRING(filename, 9, 2) + ':' + 
          SUBSTRING(filename, 11, 2) + ':' + 
          SUBSTRING(filename, 13, 2) + '.' + 
          SUBSTRING(filename, 15, 3)) AS datetime
FROM 
  your_table
