private void RibbonItem_Click(object sender, RoutedEventArgs e)
{
    // 클릭한 Ribbon 아이템에 해당하는 Document를 생성하고 추가합니다.
    Document document = new Document();
    document.Content = new YourPage(); // 여기서 YourPage는 도킹될 페이지를 나타냅니다.
    documentManager1.Documents.Add(document);

    // 도킹된 Document를 표시합니다.
    documentManager1.Show(document);
}
