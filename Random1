string variableA = "ExampleVariableValue1￦ExampleVariableValue2￦ExampleVariableValue3";
string variableB = string.Empty;

int lastIndex = variableA.LastIndexOf('￦');
if (lastIndex != -1)
{
    variableB = variableA.Substring(lastIndex + 1);
}
else
{
    // '￦' 문자가 없는 경우에 대한 예외 처리
    variableB = variableA;
}

Console.WriteLine(variableB);
