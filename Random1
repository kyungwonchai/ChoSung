Sub 복사대기처리()
    Dim 대상시트 As Worksheet
    Dim 주차2시트 As Worksheet
    Dim 마지막행 As Long
    Dim 마지막행2 As Long
    Dim i As Long, j As Long
    Dim 지역향 As String
    Dim 모델명 As String
    Dim 월화수목금(1 To 5) As String
    
    ' 시트 객체 설정
    Set 대상시트 = ThisWorkbook.Sheets("대상_시트")
    Set 주차2시트 = ThisWorkbook.Sheets("주차2_시트")
    
    ' 대상_시트의 C열의 마지막 사용 행 찾기
    마지막행 = 대상시트.Cells(Rows.Count, 3).End(xlUp).Row
    
    ' 대상_시트의 C열을 순회하면서 "복사대기" 값을 찾는다
    For i = 1 To 마지막행
        If 대상시트.Cells(i, 3).Value = "복사대기" Then
            ' D열의 값을 지역향 변수에 저장
            지역향 = 대상시트.Cells(i, 4).Value
            ' E열의 값을 모델명 변수에 저장
            모델명 = 대상시트.Cells(i, 5).Value
            ' F~J열의 값을 월화수목금 변수에 각각 저장
            For j = 1 To 5
                월화수목금(j) = 대상시트.Cells(i, 5 + j).Value
            Next j
            
            ' 주차2_시트의 C열을 순회하면서 지역향 변수를 찾는다
            마지막행2 = 주차2시트.Cells(Rows.Count, 3).End(xlUp).Row
            For j = 1 To 마지막행2
                If 주차2시트.Cells(j, 3).Value = 지역향 Then
                    ' D열이 비어있다면 그 위치에 값을 넣음
                    If IsEmpty(주차2시트.Cells(j, 4)) Then
                        주차2시트.Cells(j, 4).Value = 모델명
                        ' F~K 열에 월화수목금 값을 차례로 넣음
                        For k = 1 To 5
                            주차2시트.Cells(j, 5 + k - 1).Value = 월화수목금(k)
                        Next k
                    Else
                        ' D열이 비어있지 않다면, 비어있는 첫째 칸을 찾는다
                        While Not IsEmpty(주차2시트.Cells(j, 4))
                            j = j + 1
                        Wend
                        ' 값을 넣음
                        주차2시트.Cells(j, 4).Value = 모델명
                        ' F~K 열에 월화수목금 값을 차례로 넣음
                        For k = 1 To 5
                            주차2시트.Cells(j, 5 + k - 1).Value = 월화수목금(k)
                        Next k
                    End If
                    Exit For ' 해당 지역향을 찾으면 더 이상 순회하지 않음
                End If
            Next j
        End If
    Next i
