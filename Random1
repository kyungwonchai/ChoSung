CREATE PROCEDURE MyProcedure
AS
BEGIN
    DECLARE @currentHour INT;
    DECLARE @currentMinute INT;

    SET @currentHour = DATEPART(HOUR, GETDATE());
    SET @currentMinute = DATEPART(MINUTE, GETDATE());

    IF @currentHour = 0 AND @currentMinute BETWEEN 10 AND 20
    BEGIN
        -- 조건에 맞을 때 실행할 쿼리를 여기에 작성합니다.
        PRINT '조건에 맞습니다.';
    END
    ELSE
    BEGIN
        PRINT '조건에 맞지 않습니다.';
    END
END;
