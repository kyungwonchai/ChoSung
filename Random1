namespace AAutoHandle.Commands
{
    using System;
    using System.Diagnostics;
    using System.Threading.Tasks;

    public class OpenBrowserCommand : IMacroCommand
    {
        private readonly string _url;

        public OpenBrowserCommand(string url)
        {
            _url = url;
        }

        public async Task ExecuteAsync()
        {
            try
            {
                Process.Start("chrome.exe", _url);
                Core.MacroLogger.Instance.AddLog($"ì›¹ ë¸Œë¼ìš°ì € ì—´ê¸°: {_url}");
                await Task.Delay(2000); // ë¸Œë¼ìš°ì €ê°€ ì—´ë¦´ ì‹œê°„ì„ ê¸°ë‹¤ë¦¼
            }
            catch (Exception ex)
            {
                Core.MacroLogger.Instance.AddLog($"[Error] ë¸Œë¼ìš°ì € ì‹¤í–‰ ì‹¤íŒ¨: {ex.Message}");
            }
        }
    }
}
