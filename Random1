using System;
using System.IO;
using System.Linq;

class Program
{
    static void Main()
    {
        string path = @"your_directory_path_here";
        string searchPattern = "파일명*.log";

        DirectoryInfo di = new DirectoryInfo(path);
        FileInfo[] files = di.GetFiles(searchPattern);

        // 수정시간 내림차순으로 정렬
        var sortedFiles = files.OrderByDescending(f => f.LastWriteTime).ToArray();

        // string[] 변수에 경로 저장
        string[] sortedFilePaths = sortedFiles.Select(f => f.FullName).ToArray();

        // 출력하여 확인
        foreach (string filePath in sortedFilePaths)
        {
            Console.WriteLine(filePath);
        }
    }
}
