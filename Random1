using System;
using System.Linq;

public class Program
{
    public static void Main()
    {
        string input = "value0_value1_value2_value3_value4_value5_value6_value7_value8_value9_value10_value11_value12_value13";
        int[] indices = { 0, 5, 10, 11, 12 }; // The indices you're interested in

        // Split the string
        var splitValues = input.Split('_');

        // Select values at specified indices, ensuring we don't go out of bounds
        var selectedValues = indices.Where(index => index < splitValues.Length)
                                    .Select(index => splitValues[index])
                                    .ToArray();

        // Now, selectedValues contains the elements at the desired indices
        foreach (var value in selectedValues)
        {
            Console.WriteLine(value);
        }
    }
}
