CREATE PROCEDURE spExecuteDynamicSQL
    @Param NVARCHAR(50),
    @Result NVARCHAR(50) OUTPUT
AS
BEGIN
    DECLARE @DynamicSQL AS NVARCHAR(MAX);
    
    -- 동적 쿼리를 문자열로 구성
    SET @DynamicSQL = N'SELECT TOP 1 YourColumn FROM YourTable WHERE 조건 = ''' + @Param + '''';

    -- 동적 쿼리 실행 결과를 변수에 저장
    EXEC sp_executesql @DynamicSQL, N'@Result NVARCHAR(50) OUTPUT', @Result OUTPUT;
END;
