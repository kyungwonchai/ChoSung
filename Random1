using System;
using System.Diagnostics;

class Program
{
    static void Main()
    {
        // 지금 시간 체크 (시작)
        Stopwatch stopwatch = new Stopwatch();
        stopwatch.Start();

        // TODO: 여기에 원하는 코드를 추가하세요.

        // 지금 시간 체크 (종료)
        stopwatch.Stop();

        // 시간 차이 계산 및 표시
        double elapsedSeconds = stopwatch.Elapsed.TotalSeconds;
        Console.WriteLine($"코드 실행에 걸린 시간: {elapsedSeconds}초");
    }
}
