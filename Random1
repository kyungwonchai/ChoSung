private void barLineChart_CustomDrawSeriesPoint(object sender, CustomDrawSeriesPointEventArgs e)
{
    // 선 차트의 포인트만 고려합니다.
    if (e.Series.Name == "DDSeries3")
    {
        // 포인트의 값이 100인 경우 라벨의 배경색을 하늘색으로 변경합니다.
        if (e.SeriesPoint.Values[0] == 100)
        {
            e.LabelOptions.BackColor = Color.SkyBlue;
        }
        else
        {
            // 그렇지 않은 경우 라벨의 배경색을 노란색으로 유지합니다.
            e.LabelOptions.BackColor = Color.Yellow;
        }
    }
}
