const sql = require('mssql')

async function loadData() {
    try {
        // config 정보를 입력하세요
        let pool = await sql.connect({
            user: '유저명',
            password: '비밀번호',
            server: '서버주소',
            database: '데이터베이스명'
        })
        let result = await pool.request().query('SELECT * FROM 테이블명')

        console.log(result)
    } catch (err) {
        console.log(err)
    }
}

loadData()
