private void gridControl1_CustomRowAppearance(object sender, CustomRowAppearanceEventArgs e)
{
    int key2Value = (int)e.Row["key2"];
    if (fetchedKeys.Contains(key2Value))
    {
        e.Appearance.Background = new SolidColorBrush(Colors.Yellow); // 배경색을 노란색으로
    }
}

private void gridControl1_CellStyle(object sender, GridCellStyleEventArgs e)
{
    int key2Value = (int)((DataRowView)gridControl1.GetRow(e.RowHandle)).Row["key2"];
    if (fetchedKeys.Contains(key2Value))
    {
        e.Appearance.Foreground = new SolidColorBrush(Colors.Black); // 글자색을 검은색으로
    }
}
