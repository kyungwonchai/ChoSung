from sqlalchemy import create_engine, Table, Column, MetaData, String, Float
from sqlalchemy.sql import insert

# MSSQL에 연결. 여기서는 사용자 이름, 비밀번호, 서버 주소, 데이터베이스 이름을 적절히 바꿔주세요.
engine = create_engine('mssql+pymssql://<username>:<password>@<server>/<database>')

metadata = MetaData()
t1 = Table('t1', metadata,
           Column('a1', String),
           Column('a2', Float),
           )

# 테이블에 데이터 삽입
with engine.connect() as conn:
    ins_query = insert(t1).values(a1='abc', a2=1.245215)
    conn.execute(ins_query)
    conn.commit()
