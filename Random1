CREATE TABLE YourTableName (
    날짜 DATE,
    작업조 CHAR(1),
    라인 INT,
    모델 VARCHAR(20),
    귀책부서 VARCHAR(5),
    array INT,
    보드수T INT,
    보드수B INT,
    발생사유 VARCHAR(255),
    현장점검사항 VARCHAR(255),
    유관부서확인사항 VARCHAR(255),
    발생유형 VARCHAR(20),
    부품수 INT,
    자재코드 VARCHAR(11),
    반납수량 INT
);
