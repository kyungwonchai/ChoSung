Sub SelectSheetEndingWith()
    Dim ws As Worksheet
    Dim found As Boolean
    found = False

    ' 모든 시트를 순회
    For Each ws In ThisWorkbook.Sheets
        ' 시트 이름이 "일별 계획"으로 끝나는지 확인
        If Right(ws.Name, Len("일별 계획")) = "일별 계획" Then
            ' 해당 시트를 활성화
            ws.Activate
            found = True
            Exit For
        End If
    Next ws

    ' 해당하는 시트가 없으면 메시지 표시
    If Not found Then
        MsgBox "일별 계획으로 끝나는 시트가 없습니다.", vbInformation
    End If
End Sub
