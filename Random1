Sub CopyWSheetsToSSS()
    Dim ws As Worksheet
    Dim sssSheet As Worksheet
    Dim lastRow As Long
    Dim destinationRow As Long

    ' 'SSS' 시트가 존재하는지 확인하고 존재하면 삭제
    Application.DisplayAlerts = False
    On Error Resume Next
    Set sssSheet = ThisWorkbook.Sheets("SSS")
    If Not sssSheet Is Nothing Then
        sssSheet.Delete
    End If
    On Error GoTo 0
    Application.DisplayAlerts = True

    ' 'SSS' 시트 생성
    Set sssSheet = ThisWorkbook.Sheets.Add(After:=ThisWorkbook.Sheets(ThisWorkbook.Sheets.Count))
    sssSheet.Name = "SSS"
    destinationRow = 5

    ' 'W'로 시작하는 모든 시트를 순회하며 데이터 복사
    For Each ws In ThisWorkbook.Sheets
        If Left(ws.Name, 1) = "W" Then
            lastRow = ws.Cells(ws.Rows.Count, "B").End(xlUp).Row
            If lastRow >= 10 Then
                ws.Range("B10:B" & lastRow).Copy sssSheet.Cells(destinationRow, 2)
                destinationRow = sssSheet.Cells(sssSheet.Rows.Count, "B").End(xlUp).Row + 1
            End If
        End If
    Next ws

    ' 'W01' 시트의 6~9행을 'SSS' 시트의 1~4행에 복사
    If Not ThisWorkbook.Sheets("W01") Is Nothing Then
        ThisWorkbook.Sheets("W01").Range("A6:D9").Copy Destination:=sssSheet.Range("A1")
    End If
End Sub
