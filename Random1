Sub ProcessWorkbook(ByVal wb As Workbook, ByVal num As Integer)
    Dim sheetName As String

    ' 숫자 값에 따라 처리할 내용을 결정
    If num = 1 Then
        sheetName = "Sheet1"
    ElseIf num = 2 Then
        sheetName = "Sheet2"
    Else
        sheetName = "OtherSheet"
    End If

    ' 해당 시트에 작업 수행
    If Not wb Is Nothing Then
        If SheetExists(sheetName, wb) Then
            ' 예: 시트의 A1 셀에 메시지 입력
            wb.Sheets(sheetName).Range("A1").Value = "작업 완료: " & num
        Else
            MsgBox sheetName & " 시트가 존재하지 않습니다."
        End If
    Else
        MsgBox "유효하지 않은 워크북입니다."
    End If
End Sub

' 시트가 존재하는지 확인하는 함수
Function SheetExists(sheetName As String, wb As Workbook) As Boolean
    Dim sheet As Object
    For Each sheet In wb.Sheets
        If sheet.Name = sheetName Then
            SheetExists = True
            Exit Function
        End If
    Next sheet
    SheetExists = False
End Function
