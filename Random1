SELECT m.ind, m.stabilizedtime_b, m.stabilizedtime_b_start, 
       TIMEDIFF(m.stabilizedtime_b, m.stabilizedtime_b_start) AS time_difference
FROM smd.tbl_history AS h
JOIN smd.tbl_modelchange AS m ON COALESCE(h.m8, h.m7) = m.ind
WHERE TIME_TO_SEC(TIMEDIFF(m.stabilizedtime_b, m.stabilizedtime_b_start)) NOT BETWEEN -3020399 AND 3020399;
