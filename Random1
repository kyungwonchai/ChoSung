private void simpleButton1_Click(object sender, EventArgs e)
{
    // 다이어그램 생성
    DevExpress.XtraDiagram.Diagram diagram = new DevExpress.XtraDiagram.Diagram();

    // MSDB1 데이터베이스에 연결
    string connectionString = "Data Source=(local);Initial Catalog=MSDB1;Integrated Security=True";
    using (SqlConnection connection = new SqlConnection(connectionString))
    {
        connection.Open();

        // Table1의 diag1 컬럼에 다이어그램 저장
        string query = "UPDATE Table1 SET diag1 = @DiagramData";
        SqlCommand command = new SqlCommand(query, connection);
        command.Parameters.AddWithValue("@DiagramData", diagram.SaveToString());
        command.ExecuteNonQuery();
    }

    // 다시 불러오기
    using (SqlConnection connection = new SqlConnection(connectionString))
    {
        connection.Open();

        // Table1의 diag1 컬럼에서 다이어그램 불러오기
        string query = "SELECT diag1 FROM Table1";
        SqlCommand command = new SqlCommand(query, connection);
        string diagramData = (string)command.ExecuteScalar();

        // 불러온 다이어그램을 활용하여 작업 수행
        diagram.LoadFromString(diagramData);
    }
}
