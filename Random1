Sub FillDateSMDShift()
    Dim startDate As Date
    Dim currentDate As Date
    Dim row As Long
    Dim dateCount As Integer
    Dim i As Integer, j As Integer, k As Integer

    ' 시작 날짜를 설정
    startDate = DateValue("2024-02-01")
    dateCount = 10 ' 원하는 날짜 수

    row = 1 ' 시작 행

    ' 날짜마다 반복
    For i = 0 To dateCount - 1
        currentDate = startDate + i
        ' SMD_01 ~ SMD_12
        For j = 1 To 12
            ' Shift1 ~ Shift3
            For k = 1 To 3
                Cells(row, 1).Value = currentDate
                Cells(row, 2).Value = "SMD_" & Format(j, "00")
                Cells(row, 3).Value = "Shift" & k
                row = row + 1
            Next k
        Next j
    Next i
End Sub
