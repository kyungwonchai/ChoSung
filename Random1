-- original_table에 있는 레코드 중 backup_table에 없는 레코드만 선택
INSERT INTO backup_table (column1, column2, column3)
  SELECT original_table.column1, original_table.column2, original_table.column3
  FROM original_table
  LEFT JOIN backup_table
  ON original_table.column1 = backup_table.column1
  AND original_table.column2 = backup_table.column2
  AND original_table.column3 = backup_table.column3
  WHERE backup_table.column1 IS NULL;
