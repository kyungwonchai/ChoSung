Dictionary<string, int> myDict = new Dictionary<string, int>();
myDict["apple"] = 1;
myDict["banana"] = 2;

// 키를 사용해 값을 찾습니다.
if (myDict.ContainsKey("apple"))
{
    int value = myDict["apple"];
    Console.WriteLine($"Value for 'apple' is: {value}");
}
else
{
    Console.WriteLine("Key not found.");
}
