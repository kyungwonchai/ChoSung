private void view_CellStyle(object sender, DevExpress.Xpf.Grid.GridCellStyleEventArgs e)
{
    if (e.RowHandle < 0)
        return;

    if (e.Column.FieldName == "Key1")
    {
        object value = gridControl1.GetCellValue(e.RowHandle, e.Column);

        if (lista.Contains(value))
        {
            e.CellStyle = this.FindResource("YourYellowCellStyle") as Style;
        }
    }
}
