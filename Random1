using System;
using System.Windows.Forms;

namespace YourNamespace
{
    public partial class MainForm : Form
    {
        private Timer timer;

        public MainForm()
        {
            InitializeComponent();

            // Timer 컨트롤 생성 및 설정
            timer = new Timer();
            timer.Interval = 20000; // 20초 (단위: 밀리초)
            timer.Tick += Timer_Tick;

            // 타이머 시작
            timer.Enabled = true;
        }

        private void Timer_Tick(object sender, EventArgs e)
        {
            // 20초마다 실행되는 작업 수행
            // 이 부분에 원하는 작업을 구현합니다.
            Console.WriteLine("작업이 실행되었습니다.");
        }
    }
}
