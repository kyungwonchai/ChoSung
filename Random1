using DevExpress.Spreadsheet;
using System.Drawing.Printing;

// SpreadSheetControl 인스턴스 가져오기
SpreadsheetControl spreadsheetControl = new SpreadsheetControl();

// Workbook 객체 가져오기
IWorkbook workbook = spreadsheetControl.Document;

// 각 시트의 인쇄 설정 접근
foreach (Worksheet sheet in workbook.Worksheets)
{
    // PrintOptions 객체 가져오기
    WorksheetPrintOptions printOptions = sheet.PrintOptions;

    // PrinterSettings 객체 생성
    PrinterSettings printerSettings = new PrinterSettings();

    // 기본 Paper Source 설정 (예: 첫 번째 용지함)
    if (printerSettings.PaperSources.Count > 0)
    {
        printerSettings.DefaultPageSettings.PaperSource = printerSettings.PaperSources[0]; // 0은 첫 번째 용지함을 의미
    }

    // PrintOptions에 PrinterSettings 적용
    printOptions.PrinterSettings = printerSettings;
}

// 변경 사항 저장
spreadsheetControl.Document.SaveDocument();
