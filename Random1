def combinations(arr, n):
    # 결과를 저장할 리스트
    result = []

    # 스택을 사용하여 조합 생성
    stack = [(0, [])]

    while stack:
        index, current_combination = stack.pop()

        # 현재 조합의 길이가 n이면 결과에 추가
        if len(current_combination) == n:
            result.append(current_combination)
            continue

        # 다음 요소를 추가하여 새로운 조합 생성
        for i in range(index, len(arr)):
            new_combination = current_combination + [arr[i]]
            stack.append((i + 1, new_combination))
    
    return result

# 테스트
arr = [1, 2, 3, 4]
n = 2
combinations_list = combinations(arr, n)
for combo in combinations_list:
    print(combo)
