 private ContentControl contentControl1;
    private DocumentPanel d1;

    public YourForm()
    {
        InitializeComponent();

        // 이미 생성된 인스턴스 사용
        contentControl1 = new ContentControl();
        d1 = new DocumentPanel();

        // DocumentPanel 속성 및 설정 설정
        d1.Caption = "New Document";
        d1.ControlContainer.Controls.Add(contentControl1);
        d1.DockedAsTabbedDocument = true;

        DocumentPanel existingTab = documentGroup1.Items.OfType<DocumentPanel>()
            .FirstOrDefault(item => item == d1);

        if (existingTab != null)
        {
            documentManager1.View.ActivateDocument(existingTab);
        }
        else
        {
            // DocumentGroup에 DocumentPanel 추가
            documentGroup1.Items.Add(d1);
            documentManager1.View.ActivateDocument(d1);
        }
    }