#조합 문제 : N개를 가지고 만들수 있는 경우의 수

def f4(arr,num):
    stack=[([],0)]
    rt=[]

    while(stack):
        current,index=stack.pop()

        if num==len(current):
            rt.append(current)
            continue

        for i in range(index,len(arr)):
            stack.append((current+ [arr[i]], i+1))
    return rt

# 예시 실행
comb = f4(["muzi", "ryan", "frodo", "neo"], 2)