using System;
using System.Text.RegularExpressions;

class Program
{
    static void Main()
    {
        string pattern = @"^[A-Za-z0-9.\-]+$";
        string input = "your_string_here"; // 검사할 문자열을 여기에 넣으세요

        bool isValid = Regex.IsMatch(input, pattern);
        if (isValid)
        {
            Console.WriteLine("문자열이 조건에 맞습니다.");
        }
        else
        {
            Console.WriteLine("문자열이 조건에 맞지 않습니다.");
        }
    }
}
