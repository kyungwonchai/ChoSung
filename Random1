Sub FillCells()
    Dim startTime As Date
    Dim currentTime As Date
    Dim rValue As String
    Dim yValue As String
    Dim rowCount As Integer
    Dim i As Integer
    
    ' 시작 시간 설정 (00시 25분)
    startTime = TimeSerial(0, 25, 0)
    
    ' 각 R, Y 값과 간격 설정
    rValue = "R"
    yValue = "Y"
    
    ' 작업할 행 수 설정
    rowCount = 48 ' 24시간
    
    ' 시작 시간부터 각 행에 값을 채워넣기
    currentTime = startTime
    For i = 1 To rowCount Step 2
        Cells(i, 1).Value = rValue & Format(currentTime, "^hh^mm")
        Cells(i + 1, 1).Value = yValue & Format(currentTime + TimeSerial(0, 5, 0), "^hh^mm") ' R 시간에 5분을 더한 값
        currentTime = currentTime + TimeSerial(0, 30, 0) ' R과 R 사이에 30분 간격
    Next i
End Sub

