-- table_신청IP 테이블 생성
CREATE TABLE table_신청IP (
    ID INT PRIMARY KEY IDENTITY,
    IP VARCHAR(15) NOT NULL,
    Name VARCHAR(50) NOT NULL,
    Department VARCHAR(50) NOT NULL,
    SubmittedDate DATETIME NOT NULL DEFAULT GETDATE(),
    ApprovalStatus BIT DEFAULT 0,
    ApprovalDate DATETIME,
    CONSTRAINT CK_table_신청IP_IP UNIQUE(IP)
);

-- table_신청URL 테이블 생성
CREATE TABLE table_신청URL (
    ID INT PRIMARY KEY IDENTITY,
    URLName VARCHAR(100) NOT NULL,
    RegistrationDate DATETIME NOT NULL DEFAULT GETDATE(),
    CONSTRAINT CK_table_신청URL_URLName UNIQUE(URLName)
);
