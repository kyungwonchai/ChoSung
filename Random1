private void chartControl_MouseClick(object sender, MouseEventArgs e)
{
    ChartControl chartControl = sender as ChartControl;
    if (chartControl == null) return;

    ChartHitInfo hitInfo = chartControl.CalcHitInfo(e.X, e.Y);
    if (hitInfo.HitTest == HitTest.Series)
    {
        Series series = hitInfo.Series;
        string seriesName = series.Name;

        MessageBox.Show($"시리즈명: {seriesName}");
    }
}
