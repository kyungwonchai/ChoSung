def combinations(arr, num):
    result = []
    stack = [([], 0)]

    while stack:
        current, index = stack.pop()
        
        if len(current) == num:
            result.append(current)
            continue
        
        for i in range(index, len(arr)):
            stack.append((current + [arr[i]], i + 1))
    
    return result

comb = combinations([1, 2, 3, 4], 2)
for c in comb:
    print(c)
