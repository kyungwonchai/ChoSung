Time1DifferenceInSeconds =
VAR CurrentRowTime = MAX('YourTable'[time1])
VAR PreviousRowTime = CALCULATE(
    MAX('YourTable'[time1]),
    FILTER(
        ALL('YourTable'),
        'YourTable'[time1] < CurrentRowTime
    )
)
RETURN
    IF(
        ISBLANK(PreviousRowTime),
        BLANK(),
        DATEDIFF(PreviousRowTime, CurrentRowTime, SECOND)
    )
