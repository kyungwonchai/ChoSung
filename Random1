DataTable dataTable = new DataTable(); // 데이터 테이블을 예시로 사용합니다.
// DataTable에 필요한 컬럼들을 정의하고 데이터를 채우는 작업을 수행합니다.

// "EA" 컬럼 추가
dataTable.Columns.Add("EA", typeof(int));

// 각 DataRow에 대해 처리를 수행하고 "EA" 컬럼 업데이트
foreach (DataRow row in dataTable.Rows)
{
    // 처리 작업 수행
    // ...

    // 처리 결과를 "EA" 컬럼에 업데이트
    int result = GetProcessingResult(row); // 예시로 처리 결과 값을 얻어오는 함수를 가정합니다.
    row["EA"] = result;
}
