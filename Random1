Sub GenerateShiftScheduleBetweenDates()
    Dim ws As Worksheet
    Dim newWs As Worksheet
    Dim startDate As Date
    Dim endDate As Date
    Dim currentDate As Date
    Dim newRow As Long
    
    ' 현재 시트
    Set ws = ThisWorkbook.Sheets("Sheet1")
    
    ' 시작 날짜와 종료 날짜 읽기
    startDate = ws.Cells(1, 1).Value
    endDate = ws.Cells(1, 2).Value
    
    ' 새로운 시트 생성
    Set newWs = ThisWorkbook.Sheets.Add
    newWs.Name = "Shift Schedule"
    
    ' 헤더 추가
    newWs.Cells(1, 1).Value = "Date"
    newWs.Cells(1, 2).Value = "Shift"
    
    ' 날짜별로 시프트 추가
    newRow = 2
    currentDate = startDate
    Do While currentDate <= endDate
        newWs.Cells(newRow, 1).Value = currentDate
        newWs.Cells(newRow, 2).Value = "Shift1"
        newRow = newRow + 1
        
        newWs.Cells(newRow, 1).Value = currentDate
        newWs.Cells(newRow, 2).Value = "Shift2"
        newRow = newRow + 1
        
        newWs.Cells(newRow, 1).Value = currentDate
        newWs.Cells(newRow, 2).Value = "Shift3"
        newRow = newRow + 1
        
        currentDate = currentDate + 1
    Loop
End Sub
