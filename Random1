SELECT 
    -- Split the model column value by underscore and get the third part
    SUBSTRING(value, CHARINDEX('_', value) + 1, CHARINDEX('_', value, CHARINDEX('_', value) + 1) - CHARINDEX('_', value) - 1) AS ThirdPart,
    -- Extract the second and fourth characters from the third part
    SUBSTRING(ThirdPart, 2, 1) AS SecondChar,
    SUBSTRING(ThirdPart, 4, 1) AS FourthChar
FROM 
    -- Use a string split function to
