from openpyxl import load_workbook

# 엑셀 파일 로드
workbook = load_workbook('파일경로/파일이름.xlsx')

# 시트 선택
sheet = workbook['시트이름']

# 가져올 행 범위 지정 (예: 12번째 행부터 시작)
start_row = 12

# 모든 셀의 값을 가져오기 위한 빈 리스트 생성
cell_values = []

# 특정 범위의 행을 순회하며 셀 값을 가져와서 리스트에 추가
for row in sheet.iter_rows(min_row=start_row, values_only=True):
    # 각 행의 셀 값을 가져와서 리스트에 추가
    cell_values.append(row)

# 각 셀 값들을 활용하여 데이터 딕셔너리 생성
data = dict(zip(columns, cell_values))

# 데이터 딕셔너리 출력
print(data)
