Time1DifferenceInSeconds =
IF(
    ISBLANK('YourTable'[time1]),
    BLANK(),
    VAR CurrentIP = 'YourTable'[ip1]
    VAR PreviousRowIndex = 'YourTable'[Index] - 1
    VAR PreviousRowIP = LOOKUPVALUE('YourTable'[ip1], 'YourTable'[Index], PreviousRowIndex)
    RETURN
        IF(
            CurrentIP = PreviousRowIP,
            DATEDIFF(
                PREVIOUSMONTH('YourTable'[time1]),
                'YourTable'[time1],
                SECOND
            ),
            BLANK()
        )
)
