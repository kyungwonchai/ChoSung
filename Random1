using System;
using System.Text.RegularExpressions;

class Program
{
    static void Main()
    {
        string input = "GSM_S999U$1414dg_anotherPart";
        string pattern = @"_(.*?)(?:_|$)"; // 첫 번째 언더바 이후로 두 번째 언더바 전까지의 모든 문자를 비탐욕적으로 캡처

        Match match = Regex.Match(input, pattern);
        if (match.Success)
        {
            string matchedValue = match.Groups[1].Value; // 첫 번째 그룹에 해당하는 값을 가져옵니다.
            Console.WriteLine(matchedValue); // "S999U$1414dg" 출력
        }
    }
}
