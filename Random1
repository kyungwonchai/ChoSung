WITH LatestUpdates AS (
    SELECT
        DESCRIPTION,
        RESULT,
        STRAP,
        SHOES_R,
        SHOES_L,
        UPDATET_TIME,
        ROW_NUMBER() OVER (PARTITION BY DESCRIPTION ORDER BY UPDATETIME_TIME DESC) AS RowNum
    FROM
        ESC_CHECK_HISTORY
)
SELECT
    DESCRIPTION,
    RESULT,
    STRAP,
    SHOES_R,
    SHOES_L,
    UPDATET_TIME
FROM
    LatestUpdates
WHERE
    RowNum = 1;
