-- 임시 테이블 생성
CREATE TABLE #TempTable (ID INT, Data VARCHAR(100));

-- 원본 테이블에서 데이터를 가져와 임시 테이블에 삽입
INSERT INTO #TempTable (ID, Data)
SELECT
    ROW_NUMBER() OVER (ORDER BY [정렬기준컬럼]) AS RowNum,
    [데이터컬럼]
FROM
    [원본테이블]
ORDER BY
    [정렬기준컬럼];

-- 임시 테이블에서 데이터 확인
SELECT * FROM #TempTable;

-- 임시 테이블 삭제
DROP TABLE #TempTable;
