using System;
using System.IO;
using System.Linq;

class Program
{
    static void Main()
    {
        // 지정 시간 설정
        DateTime specifiedTime = new DateTime(2023, 9, 21, 12, 0, 0);

        // 디렉토리 경로
        string directoryPath = @"C:\example_directory\";

        // 해당 디렉토리의 모든 파일 가져오기
        string[] allFiles = Directory.GetFiles(directoryPath);

        // 지정 시간 이후에 생성된 파일만 선택
        var recentFiles = allFiles.Where(file =>
        {
            FileInfo fileInfo = new FileInfo(file);
            return fileInfo.CreationTime > specifiedTime;
        }).ToList();

        // 결과 출력
        foreach (var file in recentFiles)
        {
            Console.WriteLine(file);
        }
    }
}
