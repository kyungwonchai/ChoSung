using System;
using System.IO;
using System.Linq;

class Program
{
    static void Main()
    {
        string path = @"your_directory_path_here";
        string searchPattern = "파일명*.log";

        DirectoryInfo di = new DirectoryInfo(path);
        FileInfo[] files = di.GetFiles(searchPattern);

        // 수정시간 내림차순으로 정렬 후 첫 번째 파일 가져오기
        var latestFile = files.OrderByDescending(f => f.LastWriteTime).FirstOrDefault();

        if (latestFile != null)
        {
            Console.WriteLine(latestFile.FullName);
        }
        else
        {
            Console.WriteLine("매칭되는 파일이 없습니다.");
        }
    }
}
