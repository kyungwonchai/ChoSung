private void OnCustomCellStyle(object sender, DevExpress.Xpf.Grid.RowCellStyleEventArgs e)
{
    if (e.RowHandle < 0) return;
    if (e.Column.FieldName != "Key1") return;

    object value = e.CellValue;
    if (lista.Contains(value))
    {
        e.Cell.Appearance.Background = Brushes.Yellow;
        e.Cell.Appearance.Foreground = Brushes.Black;
    }
}