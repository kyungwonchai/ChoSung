using System;
using System.Data;
using System.Data.SqlClient;

namespace YourNamespace
{
    class Program
    {
        static void Main(string[] args)
        {
            string connectionString = "Server=your_server;Database=your_database;User Id=your_username;Password=your_password;";

            using (SqlConnection connection = new SqlConnection(connectionString))
            {
                using (SqlCommand command = new SqlCommand("sp_InsertOrUpdate", connection))
                {
                    command.CommandType = CommandType.StoredProcedure;

                    command.Parameters.Add("@N_datek", SqlDbType.Date).Value = DateTime.Now;
                    command.Parameters.Add("@C_Site_Name", SqlDbType.NVarChar).Value = "YourSiteName";
                    command.Parameters.Add("@C_Resource_Name", SqlDbType.NVarChar).Value = "YourResourceName";
                    command.Parameters.Add("@C_Basic_Name__Fix_", SqlDbType.NVarChar).Value = "YourBasicNameFix";

                    try
                    {
                        connection.Open();
                        command.ExecuteNonQuery();
                    }
                    catch (Exception ex)
                    {
                        Console.WriteLine("Error: " + ex.Message);
                    }
                    finally
                    {
                        connection.Close();
                    }
                }
            }
        }
    }
}
