Sub FindMounterWorkbook()
    Dim wb As Workbook
    Dim targetWb As Workbook
    Dim found As Boolean
    
    found = False
    
    ' ì—´ë¦° ëª¨ë“  ì›Œí¬ë¶ì„ í™•ì¸
    For Each wb In Application.Workbooks
        ' íŒŒì¼ëª…ì´ "Mounter"ë¡œ ì‹œì‘í•˜ëŠ” ê²½ìš° ì°¾ê¸° (ëŒ€ì†Œë¬¸ì êµ¬ë¶„ ì—†ìŒ)
        If LCase(Left(wb.Name, 7)) = "mounter" Then
            Set targetWb = wb
            found = True
            Exit For ' ì²« ë²ˆì§¸ë¡œ ì°¾ì€ Mounter íŒŒì¼ë§Œ ì €ì¥
        End If
    Next wb
    
    ' ê²°ê³¼ í™•ì¸
    If found Then
        MsgBox "Mounterë¡œ ì‹œì‘í•˜ëŠ” íŒŒì¼ì„ ì°¾ì•˜ìŠµë‹ˆë‹¤: " & targetWb.Name, vbInformation
