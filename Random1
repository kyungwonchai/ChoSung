SET @prev_a := NULL, @prev_b := NULL, @prev_d := NULL, @prev_e := NULL;

SELECT
    curr.a,
    curr.b,
    curr.c,
    IF(@prev_a = curr.a AND @prev_b = curr.b, @prev_d, NULL) AS prev_d,
    IF(@prev_a = curr.a AND @prev_b = curr.b, @prev_e, NULL) AS prev_e,
    @prev_a := curr.a,
    @prev_b := curr.b,
    @prev_d := curr.d,
    @prev_e := curr.e
FROM
    (SELECT * FROM your_table ORDER BY a, b, c) AS curr
