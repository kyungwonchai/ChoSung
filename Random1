public string Menuc_SelectedDate 
{ 
    get => menuc_SelectedDate; 
    set 
    { 
        menuc_SelectedDate = value; 
        OnPropertyChanged(nameof(Menuc_SelectedDate)); 
    } 
}

public string Menuc_SelectedBoardType 
{ 
    get => menuc_SelectedBoardType; 
    set 
    { 
        menuc_SelectedBoardType = value; 
        OnPropertyChanged(nameof(Menuc_SelectedBoardType)); 
    } 
}

public string Menuc_SelectedLine 
{ 
    get => menuc_SelectedLine; 
    set 
    { 
        menuc_SelectedLine = value; 
        OnPropertyChanged(nameof(Menuc_SelectedLine)); 
    } 
}

public string Menuc_SelectedStatus 
{ 
    get => menuc_SelectedStatus; 
    set 
    { 
        menuc_SelectedStatus = value; 
        OnPropertyChanged(nameof(Menuc_SelectedStatus)); 
    } 
}
