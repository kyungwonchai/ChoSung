CREATE PROCEDURE GetLineData
AS
BEGIN
    SELECT 라인 AS Category, COUNT(*) AS Quantity FROM T_현장셑척 GROUP BY 라인
END

CREATE PROCEDURE GetModelData
AS
BEGIN
    SELECT 모델 AS Category, COUNT(*) AS Quantity FROM T_현장셑척 GROUP BY 모델
END

CREATE PROCEDURE GetTypeData
AS
BEGIN
    SELECT 발생유형 AS Category, COUNT(*) AS Quantity FROM T_현장셑척 GROUP BY 발생유형
END
