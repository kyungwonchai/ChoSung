UPDATE table1
JOIN table2 ON table1.id = 49 AND table2.ind = 50
SET
  table1.a = IF(table1.a IS NULL, table2.a, table1.a),
  table1.b = IF(table1.b IS NULL, table2.b, table1.b),
  table1.c = IF(table1.c IS NULL, table2.c, table1.c),
  table1.d = IF(table1.d IS NULL, table2.d, table1.d),
  table1.e = IF(table1.e IS NULL, table2.e, table1.e)
WHERE
  table1.id = 49 AND
  (
    table1.a IS NULL OR
    table1.b IS NULL OR
    table1.c IS NULL OR
    table1.d IS NULL OR
    table1.e IS NULL
  );
