Sub MultiLevelSort()
    Dim ws As Worksheet
    Dim sortRange As Range
    Set ws = ThisWorkbook.Sheets("Sheet1")
    
    ' ğŸ“Œ ì •ë ¬ ëŒ€ìƒ ë²”ìœ„ ì„¤ì •
    Set sortRange = ws.Range("A5:K100")

    ' ğŸ“Œ ì •ë ¬ ì„¤ì • ì´ˆê¸°í™”
    ws.Sort.SortFields.Clear

    ' ğŸ“Œ ìµœëŒ€ 6ê°œ í‚¤ ì„¤ì • ì˜ˆì‹œ (A ~ F ì—´)
    ws.Sort.SortFields.Add Key:=ws.Range("A5:A100"), Order:=xlAscending
    ws.Sort.SortFields.Add Key:=ws.Range("B5:B100"), Order:=xlAscending
    ws.Sort.SortFields.Add Key:=ws.Range("C5:C100"), Order:=xlAscending
    ws.Sort.SortFields.Add Key:=ws.Range("D5:D100"), Order:=xlAscending
    ws.Sort.SortFields.Add Key:=ws.Range("E5:E100"), Order:=xlAscending
    ws.Sort.SortFields.Add Key:=ws.Range("F5:F100"), Order:=xlAscending

    ' ğŸ“Œ ì •ë ¬ ì‹¤í–‰
    With ws.Sort
        .SetRange sortRange
        .Header = xlNo ' í—¤ë”ê°€ ìˆìœ¼ë©´ xlYes
        .MatchCase = False
        .Orientation = xlTopToBottom
        .Apply
    End With