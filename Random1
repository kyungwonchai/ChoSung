from openpyxl import load_workbook

# 원본 엑셀 파일 경로
original_file_path = 'path/to/original_file.xlsx'

# 새로 저장할 엑셀 파일 경로
new_file_path = 'path/to/new_file.xlsx'

# 엑셀 파일 열기
workbook = load_workbook(filename=original_file_path)

# 원하는 작업 수행
# 예시: 시트 복사하기
source_sheet = workbook.active
new_sheet = workbook.copy_worksheet(source_sheet)

# 엑셀 파일 저장
workbook.save(filename=new_file_path)

# 엑셀 파일 닫기
workbook.close()
