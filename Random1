private void gridView_CustomDrawRow(object sender, DevExpress.XtraGrid.Views.Base.RowObjectCustomDrawEventArgs e)
{
    GridView gridView = sender as GridView;
    YourDataType rowData = e.Row as YourDataType; // YourDataType에는 행 데이터의 타입을 넣어야 합니다.
    
    if (rowData != null && Convert.ToInt32(rowData.컬럼A) > 60) // 변경하려는 컬럼의 값을 가져와서 조건을 설정합니다.
    {
        e.Appearance.BackColor = Color.Yellow; // 노랑으로 변경
    }
}
