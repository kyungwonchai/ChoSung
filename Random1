Sub MainProcedure()
    Dim startTime As Double
    Dim endTime As Double
    Dim totalTime As Double

    ' 시작 시간 기록
    startTime = Timer

    ' 다른 프로시저 호출
    Call AnotherProcedure1
    Call AnotherProcedure2
    ' ... 추가로 호출할 프로시저들 ...

    ' 종료 시간 기록
    endTime = Timer

    ' 총 실행 시간 계산 (초 단위)
    totalTime = endTime - startTime

    ' 실행 시간 표시
    MsgBox "총 실행 시간: " & totalTime & " 초", vbInformation
End Sub

Sub AnotherProcedure1()
    ' 여기에 프로시저 1의 코드
End Sub

Sub AnotherProcedure2()
    ' 여기에 프로시저 2의 코드
End Sub
