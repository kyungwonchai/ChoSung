using System;
using System.Data.SqlClient;

namespace MSSQL_Example
{
    class Program
    {
        static void Main(string[] args)
        {
            string connectionString = "Data Source=your_server_address;Initial Catalog=your_database;User ID=your_username;Password=your_password";
            string key1Value = "your_key1_value";

            using (SqlConnection conn = new SqlConnection(connectionString))
            {
                conn.Open();

                string query = "SELECT A1, A2, C3, GG5 FROM T_λ©”μΈν™•μ • WHERE Key1 = @Key1";
                
                using (SqlCommand cmd = new SqlCommand(query, conn))
                {
                    cmd.Parameters.AddWithValue("@Key1", key1Value);

                    using (SqlDataReader reader = cmd.ExecuteReader())
                    {
                        if (reader.Read())
                        {
                            string a1 = reader["A1"].ToString();
                            string a2 = reader["A2"].ToString();
                            string c3 = reader["C3"].ToString();
                            string gg5 = reader["GG5"].ToString();

                            Console.WriteLine($"A1: {a1}, A2: {a2}, C3: {c3}, GG5: {gg5}");
                        }
                        else
                        {
                            Console.WriteLine("No data found.");
                        }
                    }
                }
            }
        }
    }
}
