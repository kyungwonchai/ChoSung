Sub ListMounterWorkbooks()
    Dim wb As Workbook
    Dim fileList As String
    
    fileList = "Mounterë¡œ ì‹œì‘í•˜ëŠ” íŒŒì¼ ëª©ë¡:" & vbNewLine
    
    ' ì—´ë¦° ëª¨ë“  ì›Œí¬ë¶ ê²€ì‚¬
    For Each wb In Application.Workbooks
        If LCase(Left(wb.Name, 7)) = "mounter" Then
            fileList = fileList & wb.Name & vbNewLine
        End If
    Next wb
    
    ' ê²°ê³¼ ì¶œë ¥
    If fileList = "Mounterë¡œ ì‹œì‘í•˜ëŠ” íŒŒì¼ ëª©ë¡:" & vbNewLine Then
        MsgBox "Mounterë¡œ ì‹œì‘í•˜ëŠ” ì—´ë¦° íŒŒì¼ì´ ì—†ìŠµë‹ˆë‹¤.", vbExclamation
    Else
        MsgBox fileList, vbInformation
    End If
End Sub
