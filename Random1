import pyodbc

# 데이터베이스 파일 경로
db_file_path = 'path_to_your_database.mdb'

# ODBC 연결 문자열
conn_str = (
    r'DRIVER={Microsoft Access Driver (*.mdb, *.accdb)};'
    r'DBQ=' + db_file_path + ';'
)

try:
    # 데이터베이스에 연결
    conn = pyodbc.connect(conn_str)
    cursor = conn.cursor()

    # 예시 쿼리 실행 (테이블 목록 가져오기)
    cursor.execute("SELECT * FROM TableName")

    # 결과를 가져와서 출력
    rows = cursor.fetchall()
    for row in rows:
        print(row)

except pyodbc.Error as e:
    print("Error:", e)

finally:
    # 연결 닫기
    if 'cursor' in locals():
        cursor.close()
    if 'conn' in locals():
        conn.close()
