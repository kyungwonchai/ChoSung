using DevExpress.XtraBars.Ribbon;
using DevExpress.XtraBars;

foreach (RibbonPage ribbonPage in ribbonK.Pages)
{
    // RibbonPage 순회
    Debug.WriteLine($"Ribbon Page: {ribbonPage.Text}");
    foreach (RibbonPageGroup ribbonGroup in ribbonPage.Groups)
    {
        // RibbonPageGroup 순회
        Debug.WriteLine($"\tRibbon Group: {ribbonGroup.Text}");
        foreach (BarItemLink barItemLink in ribbonGroup.ItemLinks)
        {
            // RibbonGroup 안의 BarItemLink 순회
            if (barItemLink.Item is BarButtonItem barButtonItem)
            {
                // BarButtonItem의 텍스트 출력
                Debug.WriteLine($"\t\tBar Button Item: {barButtonItem.Caption}");
            }
        }
    }
}
