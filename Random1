SELECT
    SUBSTRING(SplitValues.value, 3, CHARINDEX(' ', SplitValues.value + ' ', CHARINDEX(' ', SplitValues.value) + 1) - 3) AS ThirdNumber
FROM
    (
        SELECT
            PARSENAME(REPLACE(A, ' ', '.'), number) AS value,
            ROW_NUMBER() OVER (ORDER BY (SELECT NULL)) AS rowNumber
        FROM
            master..spt_values
        WHERE
            number BETWEEN 1 AND LEN(A) - LEN(REPLACE(A, ' ', '')) + 1
            AND type = 'P'
    ) AS SplitValues
WHERE
    SplitValues.rowNumber = 3
