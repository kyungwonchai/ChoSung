using System;
using System.Data;
using System.Data.SqlClient;

class Program
{
    static void Main()
    {
        string connectionString = "Data Source=YourServer;Initial Catalog=YourDatabase;User ID=YourUsername;Password=YourPassword";

        // 프로시저명과 연결 문자열을 사용하여 SqlConnection 객체를 생성합니다.
        using (SqlConnection connection = new SqlConnection(connectionString))
        {
            try
            {
                connection.Open();

                // SqlCommand 객체를 생성하고 프로시저명과 연결된 SqlConnection을 지정합니다.
                using (SqlCommand command = new SqlCommand("YourProcedure", connection))
                {
                    command.CommandType = CommandType.StoredProcedure;

                    // 프로시저에 전달할 파라미터를 생성하고 값을 설정합니다.
                    SqlParameter parameter1 = new SqlParameter("@Parameter1", SqlDbType.VarChar);
                    parameter1.Value = "Value1";
                    command.Parameters.Add(parameter1);

                    SqlParameter parameter2 = new SqlParameter("@Parameter2", SqlDbType.Int);
                    parameter2.Value = 100;
                    command.Parameters.Add(parameter2);

                    // Output 파라미터를 생성합니다.
                    SqlParameter outputParameter = new SqlParameter("@OutputParameter", SqlDbType.NVarChar, 100);
                    outputParameter.Direction = ParameterDirection.Output;
                    command.Parameters.Add(outputParameter);

                    // 프로시저 실행
                    command.ExecuteNonQuery();

                    // Output 파라미터의 값을 확인
                    string outputValue = outputParameter.Value.ToString();
                    Console.WriteLine("Output Parameter Value: " + outputValue);
                }
            }
            catch (Exception ex)
            {
                Console.WriteLine("Error: " + ex.Message);
            }
        }
    }
}
