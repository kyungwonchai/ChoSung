string rundate = string.Empty;
using (SqlConnection connection = new SqlConnection(connectionString))
{
    connection.Open();
    SqlCommand command = new SqlCommand("SELECT CONVERT(VARCHAR(10), rundate, 120) AS rundate FROM Table_runlog;", connection);
    rundate = (string)command.ExecuteScalar();
}

DateTime parsedRundate;
if (DateTime.TryParse(rundate, out parsedRundate))
{
    DateTime threeDaysAgo = DateTime.Today.AddDays(-3);
    bool isGreaterThanThreeDaysAgo = parsedRundate > threeDaysAgo;
    // isGreaterThanThreeDaysAgo를 사용하여 판단하거나 처리합니다.
}
else
{
    // rundate를 파싱할 수 없는 경우 예외 처리
}
