SELECT
    SUBSTRING(split_values.value, CHARINDEX(' ', split_values.value, CHARINDEX(' ', split_values.value) + 1) + 1, LEN(split_values.value)) AS ThirdNumber
FROM
    (
        SELECT
            value,
            ROW_NUMBER() OVER (ORDER BY (SELECT NULL)) AS rowNumber
        FROM
        (
            SELECT 
                value, 
                ROW_NUMBER() OVER (ORDER BY (SELECT NULL)) AS rn
            FROM 
                STRING_SPLIT(A, ' ')
            WHERE 
                RTRIM(value) <> ''
        ) AS SplitValues
        WHERE
            rowNumber = 3
    ) AS split_values
