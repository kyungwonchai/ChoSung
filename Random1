using System;
using System.Collections.Generic;
using System.Linq; // LINQ 네임스페이스를 사용

class Program
{
    static void Main()
    {
        List<string> items = new List<string> { "example", "test1", "testing", "sample", "test2" };
        string startValue = "test";

        // 조건에 맞는 모든 항목을 찾습니다.
        List<string> filteredItems = items.Where(item => item.StartsWith(startValue)).ToList();

        Console.WriteLine("Items that start with '" + startValue + "':");
        foreach (var item in filteredItems)
        {
            Console.WriteLine(item);
        }
    }
}
