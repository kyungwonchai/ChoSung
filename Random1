public async Task AddTaskSequenceToCQ()
{
    foreach(FunctionStatus f in flist)
    {
        try
        {
            // 비동기 작업을 정확히 기다림
            cr c2 = await f1.addasyncTaskCommand(f);
            // 작업 성공 시 로그 또는 처리 코드
            Console.WriteLine($"Task completed for {f.FunctionName}");
        }
        catch (Exception ex)
        {
            // 예외 발생 시 로그 기록
            Console.WriteLine($"Exception occurred: {ex.Message}");
            // 예외 처리 로직 추가 가능 (예: 재시도, 실패 처리 등)
        }
    }
}
