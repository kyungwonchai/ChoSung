using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        List<Recipient> rrr = new List<Recipient>();
        rrr.Add(new Recipient { emailAddress = "주소1", recipientType = "구분1" });
        rrr.Add(new Recipient { emailAddress = "주소2", recipientType = "구분2" });

        List<object> recipients = rrr.ConvertAll(ee => new { emailAddress = ee.emailAddress, recipientType = ee.recipientType });

        foreach (var recipient in recipients)
        {
            Console.WriteLine($"emailAddress: {recipient.emailAddress}, recipientType: {recipient.recipientType}");
        }
    }
}

class Recipient
{
    public string emailAddress { get; set; }
    public string recipientType { get; set; }
}
