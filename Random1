private void Button1_Click(object sender, RoutedEventArgs e)
{
    AddDockingItem("Pagegg1", new Pagegg1());
}

private void AddDockingItem(string title, Page page)
{
    var navFrame = new DevExpress.Xpf.WindowsUI.NavigationFrame();
    navFrame.Navigate(page);
    var documentPanel = new DocumentPanel { Caption = title };
    documentPanel.Content = navFrame;
    documentGroup.Items.Add(documentPanel);
}
