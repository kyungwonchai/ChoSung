import pandas as pd

# 빈 DataFrame 생성 (컬럼명 지정)
df = pd.DataFrame(columns=["barcode", "model"])

# 새로운 값 쌍 추가
data1 = ("123456789", "ABC123")
data2 = ("987654321", "XYZ789")

# append 방식으로 데이터 추가
df = df.append(pd.DataFrame([data1], columns=df.columns), ignore_index=True)
df = df.append(pd.DataFrame([data2], columns=df.columns), ignore_index=True)

# 결과 출력
print(df)
