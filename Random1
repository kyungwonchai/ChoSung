private void gridView1_CustomDrawCell(object sender, DevExpress.XtraGrid.Views.Base.RowCellCustomDrawEventArgs e)
{
    if (e.Column.FieldName == "날짜A" && e.CellValue is DateTime dateTime)
    {
        string formattedValue = dateTime.ToString("yyyy-MM-dd HH:mm:ss");
        e.DisplayText = formattedValue;
    }
}
