Time1DifferenceInSeconds =
VAR CurrentRowTime = 'YourTable'[time1]
VAR PreviousRowIndex = 'YourTable'[Index] - 1
VAR PreviousRowTime = LOOKUPVALUE('YourTable'[time1], 'YourTable'[Index], PreviousRowIndex)
RETURN
    IF(
        ISBLANK(PreviousRowTime),
        BLANK(),
        DATEDIFF(PreviousRowTime, CurrentRowTime, SECOND)
    )
