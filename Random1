using System;
using System.Data.SqlClient;

class Program
{
    static void Main()
    {
        string connectionString = "Data Source=YourServerName;Initial Catalog=YourDatabaseName;User ID=YourUsername;Password=YourPassword";

        // 연결 문자열을 사용하여 데이터베이스 연결
        using (SqlConnection connection = new SqlConnection(connectionString))
        {
            connection.Open();

            // 삽입할 serial 값
            string serialValue = "YourSerialValue";

            // INSERT 문을 실행하는 SqlCommand 객체 생성
            using (SqlCommand command = new SqlCommand("INSERT INTO 로그테이블1 (serial) VALUES (@SerialValue)", connection))
            {
                // 파라미터 추가
                command.Parameters.AddWithValue("@SerialValue", serialValue);

                // 쿼리 실행
                command.ExecuteNonQuery();

                Console.WriteLine("데이터가 성공적으로 삽입되었습니다.");
            }
        }
    }
}
