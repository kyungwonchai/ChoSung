using System;
using System.Collections.Generic;
using System.Linq;

class Program
{
    static void Main()
    {
        // Example list
        List<string> list1 = new List<string> { "abc", "def", "abc", "ghi", "abc" };

        // Count occurrences of "abc"
        int count = list1.Count(element => element == "abc");

        Console.WriteLine($"'abc' occurs {count} times in the list.");
    }
}
