from openpyxl import load_workbook

# 기존 파일 경로
file_path = r"C:\Users\Me\Desktop\test.xlsx"  # 존재하는 파일로 수정

# 엑셀 열기
wb = load_workbook(file_path)
ws = wb.active  # 기본 시트 선택

# 셀에 값 쓰기
ws["A1"] = "Hello"
ws["B1"] = 123
ws["C1"] = "? Success"

# 저장
wb.save(file_path)

print("파일에 성공적으로 값이 입력되고 저장되었습니다.")
