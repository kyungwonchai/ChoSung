## 4. ìµœì¢… C# ì½”ë“œ (MainWindow.xaml.cs)
ê°€ì¥ ì¤‘ìš”í•œ ë¶€ë¶„ì…ë‹ˆë‹¤. ê¸°ì¡´ MainWindow.xaml.cs ì½”ë“œë¥¼ ëª¨ë‘ ì§€ìš°ê³  ì•„ë˜ì˜ ìµœì¢… ì½”ë“œë¡œ êµì²´í•˜ì„¸ìš”. ë°ì´í„° ì²˜ë¦¬ ë° DB ì—°ë™ ë¡œì§ì´ ëª¨ë‘ í¬í•¨ë˜ì–´ ìˆìŠµë‹ˆë‹¤.

C#

using System;
using System.IO;
using System.Runtime.InteropServices;
using System.Text;
using System.Text.RegularExpressions;
using System.Windows;
using System.Windows.Media.Imaging;
using System.Windows.Threading;
using Tesseract;
using funcenvget.Properties;
using System.Drawing; // System.Drawing.Common NuGet íŒ¨í‚¤ì§€
using System.Drawing.Imaging;
using MySql.Data.MySqlClient; // MySQL Connector ì‚¬ìš©

namespace funcenvget
{
    public partial class MainWindow : Window
    {
        // P/Invoke ì„ ì–¸ (ì´ì „ê³¼ ë™ì¼)
        [DllImport("user32.dll", SetLastError = true)]
        private static extern IntPtr FindWindow(string lpClassName, string lpWindowName);
        [DllImport("user32.dll")]
        [return: MarshalAs(UnmanagedType.Bool)]
        private static extern bool GetWindowRect(IntPtr hWnd, out RECT lpRect);
        [DllImport("user32.dll")]
        private static extern bool PrintWindow(IntPtr hWnd, IntPtr hdcBlt, int nFlags);
        [DllImport("user32.dll")]
        [return: MarshalAs(UnmanagedType.Bool)]
        private static extern bool IsIconic(IntPtr hWnd);
        [StructLayout(LayoutKind.Sequential)]
        public struct RECT { public int Left, Top, Right, Bottom; }

        private DispatcherTimer timer;
        private TesseractEngine ocrEngine;

        public MainWindow()
        {
            InitializeComponent();
            InitializeApp();
        }
        
        // ... (InitializeApp, btnSelectArea_Click, UpdatePreviewImage, BitmapToImageSource ë“± ì´ì „ í•¨ìˆ˜ë“¤ì€ ê·¸ëŒ€ë¡œ ìœ ì§€)
        // ... ì—¬ê¸°ì— ì´ì „ ë‹µë³€ì˜ í•´ë‹¹ í•¨ìˆ˜ë“¤ì„ ë¶™ì—¬ë„£ìœ¼ì„¸ìš”. ì½”ë“œê°€ ë„ˆë¬´ ê¸¸ì–´ì ¸ ìƒëµí•©ë‹ˆë‹¤.

        // OCR ì¸ì‹ í›„ ë°ì´í„° ì²˜ë¦¬ ë° DB ì—…ë¡œë“œë¥¼ í˜¸ì¶œí•˜ëŠ” í•µì‹¬ ë©”ì†Œë“œ
        private void ProcessOcrResult(string rawText)
        {
            // 1. '-' ê¸°í˜¸ ì œê±°
            string processedText = rawText.Replace("-", "");

            // 2. ê³µë°±ìœ¼ë¡œ ë¶„ë¦¬
            string[] parts = processedText.Split(new[] { ' ', '\n', '\r', '\t' }, StringSplitOptions.RemoveEmptyEntries);

            // 3. ë¶„ë¦¬ëœ ê°’ì´ ì •í™•íˆ 2ê°œì¸ì§€ í™•ì¸
            if (parts.Length != 2)
            {
                lblStatus.Text = $"ìƒíƒœ: ê°’ ë¶„ë¦¬ ì˜¤ë¥˜ (ì¸ì‹ëœ ê°’ {parts.Length}ê°œ)";
                return;
            }

            // 4. ìˆ«ì ë³€í™˜ ë° ì†Œìˆ˜ì  1ìë¦¬ë¡œ ìë¥´ê¸°
            if (decimal.TryParse(parts[0], out decimal val1) && decimal.TryParse(parts[1], out decimal val2))
            {
                decimal temp = Math.Truncate(val1 * 10) / 10m;
                decimal humid = Math.Truncate(val2 * 10) / 10m;

                // 5. DBì— ì—…ë¡œë“œ
                UploadToDatabase(temp, humid);
            }
            else
            {
                lblStatus.Text = $"ìƒíƒœ: ìˆ«ì ë³€í™˜ ì˜¤ë¥˜ ({parts[0]},{parts[1]})";
            }
        }

        // MySQL DBì— ë°ì´í„°ë¥¼ ì—…ë¡œë“œí•˜ëŠ” í•¨ìˆ˜
        private void UploadToDatabase(decimal temp, decimal humid)
        {
            string pcName = txtPcName.Text;
            if (string.IsNullOrWhiteSpace(pcName))
            {
                lblStatus.Text = "ìƒíƒœ: PC ì´ë¦„ì´ ë¹„ì–´ìˆì–´ DB ì‘ì—…ì„ ê±´ë„ˆëœë‹ˆë‹¤.";
                return;
            }

            string connectionString = $"Server={txtDbServer.Text};Port={txtDbPort.Text};Database={txtDbName.Text};Uid={txtDbUser.Text};Pwd={txtDbPassword.Password};";
            MySqlConnection conn = null;
            MySqlTransaction trans = null;

            try
            {
                conn = new MySqlConnection(connectionString);
                conn.Open();
                trans = conn.BeginTransaction();

                using (MySqlCommand cmd = new MySqlCommand())
                {
                    cmd.Connection = conn;
                    cmd.Transaction = trans;

                    // ì¿¼ë¦¬ 1: smd.repair_func_now í…Œì´ë¸”ì— UPDATE (ì—†ìœ¼ë©´ INSERT)
                    // ON DUPLICATE KEY UPDATE êµ¬ë¬¸ìœ¼ë¡œ í•œë²ˆì— ì²˜ë¦¬
                    cmd.CommandText = "INSERT INTO smd.repair_func_now (line, temp1, humid1, stdtime) VALUES (@line, @temp1, @humid1, NOW()) " +
                                      "ON DUPLICATE KEY UPDATE temp1 = @temp1, humid1 = @humid1, stdtime = NOW();";
                    
                    cmd.Parameters.AddWithValue("@line", pcName);
                    cmd.Parameters.AddWithValue("@temp1", temp);
                    cmd.Parameters.AddWithValue("@humid1", humid);
                    cmd.ExecuteNonQuery();

                    // ì¿¼ë¦¬ 2: smd.repair_func_new_log í…Œì´ë¸”ì— INSERT
                    cmd.CommandText = "INSERT INTO smd.repair_func_new_log (line, temp1, humid1, stdtime) VALUES (@line, @temp1, @humid1, NOW());";
                    // íŒŒë¼ë¯¸í„°ëŠ” ì´ë¯¸ ì¶”ê°€ë˜ì–´ ìˆìœ¼ë¯€ë¡œ ì¬ì‚¬ìš©
                    cmd.ExecuteNonQuery();
                }

                trans.Commit();
                lblStatus.Text = $"ìƒíƒœ: {DateTime.Now:T} - DB ì—…ë°ì´íŠ¸ ì„±ê³µ!";
            }
            catch (Exception ex)
            {
                trans?.Rollback(); // ì˜¤ë¥˜ ë°œìƒ ì‹œ íŠ¸ëœì­ì…˜ ë¡¤ë°±
                lblStatus.Text = $"ìƒíƒœ: DB ì˜¤ë¥˜! {ex.Message}";
            }
            finally
            {
                conn?.Close();
            }
        }

        // PerformLogging ìˆ˜ì •: LogData ëŒ€ì‹  ProcessOcrResult í˜¸ì¶œ
        private void PerformLogging()
        {
            try
            {
                IntPtr hWnd = FindWindow(null, txtWindowTitle.Text);
                if (hWnd == IntPtr.Zero) { /* ... */ return; }
                if (IsIconic(hWnd)) { /* ... */ return; }

                GetWindowRect(hWnd, out RECT rect);
                using (var bmp = new Bitmap(rect.Right - rect.Left, rect.Bottom - rect.Top, System.Drawing.Imaging.PixelFormat.Format32bppArgb))
                {
                    // ... (ìº¡ì²˜ ë¡œì§ì€ ë™ì¼)
                    using (var g = Graphics.FromImage(bmp)) { PrintWindow(hWnd, g.GetHdc(), 2); g.ReleaseHdc(); }

                    var cropArea = new System.Drawing.Rectangle(int.Parse(txtX.Text), int.Parse(txtY.Text), int.Parse(txtWidth.Text), int.Parse(txtHeight.Text));
                    using (var croppedBmp = bmp.Clone(cropArea, bmp.PixelFormat))
                    {
                        using (var page = ocrEngine.Process(croppedBmp))
                        {
                            string rawText = page.GetText().Trim();
                            if (!string.IsNullOrWhiteSpace(rawText))
                            {
                                // [ìˆ˜ì •] íŒŒì¼ ë¡œê¹… ëŒ€ì‹  DB ì—…ë¡œë“œ í”„ë¡œì„¸ìŠ¤ í˜¸ì¶œ
                                ProcessOcrResult(rawText);
                            }
                            else
                            {
                                lblStatus.Text = "ìƒíƒœ: í…ìŠ¤íŠ¸ ì¸ì‹ ì‹¤íŒ¨";
                            }
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                lblStatus.Text = $"ìƒíƒœ: ìº¡ì²˜ ì˜¤ë¥˜! {ex.Message}";
            }
        }

        // ì„¤ì • ë¡œë“œ/ì €ì¥ ë¡œì§ ìˆ˜ì •
        private void Window_Loaded(object sender, RoutedEventArgs e)
        {
            // ê¸°ì¡´ ë¡œì§ì— ì¶”ê°€
            txtPcName.Text = Settings.Default.PcName;
            txtDbServer.Text = Settings.Default.DbServer;
            txtDbPort.Text = Settings.Default.DbPort;
            txtDbUser.Text = Settings.Default.DbUser;
            txtDbName.Text = Settings.Default.DbName;
            // ... (ê¸°ì¡´ ì¢Œí‘œ, ì°½ì œëª© ë“± ë¡œë“œ)
        }

        private void Window_Closing(object sender, System.ComponentModel.CancelEventArgs e)
        {
            // ê¸°ì¡´ ë¡œì§ì— ì¶”ê°€
            Settings.Default.PcName = txtPcName.Text;
            Settings.Default.DbServer = txtDbServer.Text;
            Settings.Default.DbPort = txtDbPort.Text;
            Settings.Default.DbUser = txtDbUser.Text;
            Settings.Default.DbName = txtDbName.Text;
            // ... (ê¸°ì¡´ ì¢Œí‘œ, ì°½ì œëª© ë“± ì €ì¥)
            Settings.Default.Save();
        }
    }
}
â€» ìœ„ ì½”ë“œì—ì„œ ìƒëµëœ ë¶€ë¶„(InitializeApp, btnSelectArea_Click ë“±)ì€ ì´ì „ ë‹µë³€ì˜ ì½”ë“œë¥¼ ê·¸ëŒ€ë¡œ ì‚¬ìš©í•˜ì‹œë©´ ë©ë‹ˆë‹¤. PerformLogging í•¨ìˆ˜ëŠ” í•µì‹¬ ìˆ˜ì •ì‚¬í•­ì„ ë°˜ì˜í•˜ì—¬ ë‹¤ì‹œ ë„£ì–´ë“œë ¸ìŠµë‹ˆë‹¤.

## 5. MySQL í…Œì´ë¸” ì¤€ë¹„ ğŸ“œ
í”„ë¡œê·¸ë¨ì´ ì •ìƒì ìœ¼ë¡œ ë™ì‘í•˜ë ¤ë©´, MySQL ë°ì´í„°ë² ì´ìŠ¤(smd)ì— ì•„ë˜ ë‘ í…Œì´ë¸”ì´ ë¯¸ë¦¬ ìƒì„±ë˜ì–´ ìˆì–´ì•¼ í•©ë‹ˆë‹¤.

repair_func_now í…Œì´ë¸”ì˜ line ì»¬ëŸ¼ì€ PRIMARY KEY ë˜ëŠ” UNIQUE KEYë¡œ ì§€ì •ë˜ì–´ì•¼ ON DUPLICATE KEY UPDATE êµ¬ë¬¸ì´ ì •ìƒ ë™ì‘í•©ë‹ˆë‹¤.
ì•„ë˜ SQL ìŠ¤í¬ë¦½íŠ¸ë¥¼ ì‚¬ìš©í•˜ì—¬ í…Œì´ë¸”ì„ ìƒì„±í•˜ì„¸ìš”.

SQL

-- í˜„ì¬ ìƒíƒœë¥¼ ì—…ë°ì´íŠ¸í•˜ê¸° ìœ„í•œ í…Œì´ë¸” (lineì„ PKë¡œ ì„¤ì •)
CREATE TABLE smd.repair_func_now (
    `line` VARCHAR(10) NOT NULL,
    `temp1` DECIMAL(5,1) NULL,
    `humid1` DECIMAL(5,1) NULL,
    `stdtime` DATETIME NULL,
    PRIMARY KEY (`line`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

-- ëª¨ë“  ë°ì´í„°ë¥¼ ê¸°ë¡í•˜ê¸° ìœ„í•œ ë¡œê·¸ í…Œì´ë¸”
CREATE TABLE smd.repair_func_new_log (
    `id` INT NOT NULL AUTO_INCREMENT,
    `line` VARCHAR(10) NULL,
    `temp1` DECIMAL(5,1) NULL,
    `humid1` DECIMAL(5,1) NULL,
    `stdtime` DATETIME NULL,
    PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

## 6. ì‚¬ìš© ë°©ë²•
í”„ë¡œê·¸ë¨ì„ ì‹¤í–‰í•©ë‹ˆë‹¤.
PC ì´ë¦„ì„ 10ì ì´ë‚´ë¡œ ì…ë ¥í•©ë‹ˆë‹¤.
ëŒ€ìƒ ì°½ ì œëª©ì„ ì…ë ¥í•˜ê³  [ì˜ì—­ ì„¤ì •] ë²„íŠ¼ìœ¼ë¡œ ìº¡ì²˜í•  ì˜ì—­ì„ ì§€ì •í•©ë‹ˆë‹¤.
MySQL ì ‘ì† ì„¤ì •ì— ì„œë²„ IP, í¬íŠ¸, ì‚¬ìš©ì ID, ë¹„ë°€ë²ˆí˜¸, DB ì´ë¦„ì„ ì •í™•íˆ ì…ë ¥í•©ë‹ˆë‹¤.