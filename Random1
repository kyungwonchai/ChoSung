# 빈 리스트 생성
my_list = []

# 리스트에 요소 추가
my_list.append(10)
my_list.append(20)
my_list.append(30)

# 리스트의 요소 접근
print(my_list[0])  # 10 출력
print(my_list[1])  # 20 출력
print(my_list[2])  # 30 출력

# 리스트의 길이 확인
print(len(my_list))  # 3 출력

# 리스트의 요소 변경
my_list[1] = 25
print(my_list)  # [10, 25, 30] 출력

# 리스트의 요소 삭제
del my_list[0]
